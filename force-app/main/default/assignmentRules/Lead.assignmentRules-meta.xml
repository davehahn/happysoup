<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Assign to Dealer Queue</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>Dealer_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.hubspot_Nearest_Dealer_to_Prospect__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Whitefish_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>CONTAINS(Closest_Partner__r.Name, &quot;Legend Boats Whitefish&quot;)</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Barrie_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>CONTAINS(Closest_Partner__r.Name, &quot;Legend Boats Barrie&quot;)</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Montreal_Leads</assignedTo>
            <assignedToType>Queue</assignedToType>
            <formula>CONTAINS(Closest_Partner__r.Name, &quot;Legend Boats Montreal&quot;)</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>jdavis@legendboats.com</assignedTo>
            <assignedToType>User</assignedToType>
            <formula>AND( CONTAINS(Closest_Partner__r.Owner.FirstName, &quot;Jesse&quot;), CONTAINS(Closest_Partner__r.Owner.LastName, &quot;Davis&quot;) )</formula>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>jdavis@legendboats.com</assignedTo>
            <assignedToType>User</assignedToType>
            <formula>AND( CONTAINS(Closest_Partner__r.Owner.FirstName, &quot;Robert&quot;), CONTAINS(Closest_Partner__r.Owner.LastName, &quot;Beaucage&quot;) )</formula>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
