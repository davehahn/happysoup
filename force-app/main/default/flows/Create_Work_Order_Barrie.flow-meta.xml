OR( ISPICKVAL({!myVariable_current.Pickup_Location__c} , &quot;Whitefish - Pick-up&quot; ) , ISPICKVAL( {!myVariable_current.Pickup_Location__c} , &quot;Whitefish - Delivery&quot; )),
