    <description>Notifying those in Barrie to make a Work Order</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (ISPICKVAL([Opportunity].StageName, &quot;Pending Work Order&quot;)  , 
OR( ISPICKVAL([Opportunity].Pickup_Location__c , &quot;Barrie - Pick-up&quot; ) , ISPICKVAL( [Opportunity].Pickup_Location__c , &quot;Barrie - Delivery&quot; )),
OR ( ISPICKVAL ([Opportunity].Items_Shipped__c  , &quot;Items to be Shipped to Barrie&quot;) ,ISPICKVAL([Opportunity].Items_Shipped__c  , &quot;Ready to be Shipped to Barrie&quot;)))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND (ISPICKVAL({!myVariable_current.StageName}, &quot;Pending Work Order&quot;)  , 
OR( ISPICKVAL({!myVariable_current.Pickup_Location__c} , &quot;Barrie - Pick-up&quot; ) , ISPICKVAL( {!myVariable_current.Pickup_Location__c} , &quot;Barrie - Delivery&quot; )),
OR ( ISPICKVAL ({!myVariable_current.Items_Shipped__c}  , &quot;Items to be Shipped to Barrie&quot;) ,ISPICKVAL({!myVariable_current.Items_Shipped__c}  , &quot;Ready to be Shipped to Barrie&quot;)))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND (ISPICKVAL([Opportunity].StageName, &quot;Pending Work Order&quot;)  , 
OR( ISPICKVAL([Opportunity].Pickup_Location__c , &quot;Whitefish - Pick-up&quot; ) , ISPICKVAL( [Opportunity].Pickup_Location__c , &quot;Whitefish - Delivery&quot; )),
