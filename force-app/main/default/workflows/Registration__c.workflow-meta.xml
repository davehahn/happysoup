<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>Dealer_Care_Email_on_Registration_by_Dealer_to_Customer</fullName>
        <ccEmails>dealercare@legendboats.com</ccEmails>
        <description>Dealer Care Email on Registration by Dealer to Customer</description>
        <protected>false</protected>
        <recipients>
            <recipient>jdavis@legendboats.com</recipient>
            <type>user</type>
        </recipients>
        <senderType>DefaultWorkflowUser</senderType>
        <template>Dealer_Templates/New_Registration_by_Dealer</template>
    </alerts>
    <rules>
        <fullName>Notify Dealer Team on New Registration by Dealer</fullName>
        <actions>
            <name>Dealer_Care_Email_on_Registration_by_Dealer_to_Customer</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Registration__c.CreatedById</field>
            <operation>notContain</operation>
            <value>Hahn,gimbal,corcoran,maclean,davis,beaucage,lavallee,fram,dewar,boothe,duhamel,rubel,parise,paulino,lockhart,roy</value>
        </criteriaItems>
        <criteriaItems>
            <field>Registration__c.Product_Name__c</field>
            <operation>notContain</operation>
            <value>Trolling,DUV,DUP,DUB,terrova</value>
        </criteriaItems>
        <description>Notifies Dealer Team when there is a new registration by a dealer to one of their customers.</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
