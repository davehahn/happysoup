    function sortDateTime(rowData1, rowData2, dataIndx)
    {
        var data1Str = rowData1[dataIndx].split('/'),
            data2Str = rowData2[dataIndx].split('/'),
            data1 = data1Str[1] + '/' + data1Str[0] + '/' + data1Str[2],
            data2 = data2Str[1] + '/' + data2Str[0] + '/' + data2Str[2],
            date1 = new Date(data1),
            date2 = new Date(data2),
            time1 = date1.getTime(),
            time2 = date2.getTime();

        if (time1 > time2) {
            return 1;
        }
        else if (time1 < time2) {
            return -1;
        }
        else {
            return 0;
        }
    }
