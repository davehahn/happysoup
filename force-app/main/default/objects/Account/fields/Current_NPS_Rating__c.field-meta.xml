<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Current_NPS_Rating__c</fullName>
    <description>Used to calculate NPS Score in reporting</description>
    <externalId>false</externalId>
    <formula>IF( Current_NPS_Score__pc &gt;8, 1,  IF( Current_NPS_Score__pc &lt;7, -1, 0))</formula>
    <label>Current NPS Rating</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
