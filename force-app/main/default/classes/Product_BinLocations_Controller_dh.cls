public with sharing class Product_BinLocations_Controller_dh {

  public class BinLocationWrapper {
    public String userWarehouse { get; set; }
    public Map<String, List<Map<String, String>>> binLocations { get; set; }
    public String imageURL { get; set; }
    public String productName { get; set; }
    public String productFamily { get; set; }
    public String productCode { get; set; }
    public String productDescription { get; set;}

    public BinLocationWrapper( Product2 p )
    {
      this.productName = p.Name;
      this.productCode = p.ProductCode;
      this.productDescription = p.Description;
      this.productFamily = p.Family;
      this.imageURL = p.Default_Image_Thumb__c;
      findUserWarehouse();
      buildBinLocations( p.Bin_Locations__r );
    }

    private void findUserWarehouse()
    {
      User u = [SELECT Id, Warehouse__c FROM User WHERE Id = :UserInfo.getUserId() ];
      this.userWarehouse = u.Warehouse__c == null ? 'Whitefish' : u.Warehouse__c;
    }

    private void buildBinLocations( Product_Bin_Location__c[] binLocs )
    {
      this.binLocations = new Map<String, List<Map<String, String>>>();
      for( String whName : LGND_Utilities.getPicklistValues('User', 'Warehouse__c') )
      {
        this.binLocations.put( whName, new List<Map<String, String>>() );
      }
      if( binLocs != null && binLocs.size() > 0 )
      {
        for( Product_Bin_Location__c pbl : binLocs )
        {
          if( this.binLocations.containsKey( pbl.Warehouse__r.Name ) )
          {
            Map<String, String> r = new Map<String, String>();
            r.put('Bin', pbl.Name);
            r.put('Aisle', pbl.Aisle__c);
            r.put('Section', pbl.Section__c);
            this.binLocations.get(  pbl.Warehouse__r.Name ).add( r );
          }
        }
      }
    }

  }

  @AuraEnabled
  public static String fetchBinLoactionDetails( String prodId )
  {
    Product2 p = [SELECT Id,
                         Name,
                         Description,
                         Family,
                         Default_Image_Thumb__c,
                         ProductCode,
                  (SELECT Id,
                          Aisle__c,
                          Name, Section__c,
                          Warehouse__c,
                          Warehouse__r.Name
                   FROM Bin_Locations__r)
                FROM Product2
                WHERE Id = :prodId ];
    return JSON.serialize( new BinLocationWrapper( p ) );
  }

  @AuraEnabled
  public static String fetchWarehouseInventory( Id prodId, String whName )
  {
      Set<Id> prodIds = new Set<Id>{prodId};
      Id whId = null;
      try {
        AcctSeedERP__Warehouse__c wh = [SELECT Id FROM AcctSeedERP__Warehouse__c WHERE Name = :whName];
        whId = wh.Id;
      }
      catch(Exception e){}
      //Id partRTid = LGND_Utilities.recordTypeIdFromName('Product2', 'Part');
      return JSON.serialize( gcInventoryReport_MRP.retrieveProducts( prodIds, whId ) );
      //Map<Id, gcInventoryReport_MRP.cube> allCubes = gcInventoryReport_MRP.retrieveProducts(setIdProduct,idWarehouse);
  }

