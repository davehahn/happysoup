public with sharing class AWS_S3_Object_TriggerHandler {
  private Boolean m_isExecuting = false;
  private Integer batchSize = 0;
  private S3.AmazonS3 as3;

	public AWS_S3_Object_TriggerHandler(Boolean isExecuting, Integer size) {
    m_isExecuting = isExecuting;
    batchSize = size;
  }

  public void deleteFromAWS(Map<Id, AWS_S3_Object__c> s3Objects){

    for(AWS_S3_Object__c s3Object : s3Objects.values())
    {

      if( !Test.isRunningTest() )
      {
        setupDelete(s3Object.Bucket_Name__c, s3Object.Path__c + '/' + s3Object.File_Name__c );
        for( String size : new Set<String>{'sf_gallery', '500x500', '700w', '1280w'} )
        {
          setupDelete(s3Object.Bucket_Name__c, s3Object.Path__c + '/' + size + '/' + s3Object.File_Name__c );
        }
      }

    }

  }

  @future (callout=true)
  private static void setupDelete(String bucket, String key){
    try {
      String awsKey = AWS_Settings__c.getOrgDefaults().key__c;
      String awsSecret = AWS_Settings__c.getOrgDefaults().secret__c;
      S3.AmazonS3 as3 = new S3.AmazonS3(awsKey,awsSecret);
      performObjectDelete(as3, bucket, key);
    }
    catch(Exception e){
      System.debug('Caught exception in AWS S3 Trigger handler ' + e.getMessage());
    }

  }

   private static void performObjectDelete(S3.AmazonS3 as3, String bucket, String key){
    try{
      Datetime now = Datetime.now();
      S3.Status deleteObjectResult= as3.DeleteObject(bucket,key,as3.key,now,as3.signature('DeleteObject',now), as3.secret);
      System.debug('Successfully deleted a S3 Object: ' + deleteObjectResult.Description);
    }
    catch(System.CalloutException callout){
      System.debug('CALLOUT EXCEPTION: ' + callout);
     }
    catch(Exception ex){
      System.debug(ex);
     }
  }

