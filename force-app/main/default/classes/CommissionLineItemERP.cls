  public static void createRecords( Set<Id> materialIds )
  {
    Map<Id, List<String>> changesMap = new Map<Id, List<String>>();
    CommissionLineItem__c[] clis = buildNewRecords( materialIds );
    insert clis;
    for( CommissionLineItem__c cli : clis )
    {
      if( !changesMap.containsKey( cli.CommissionRecord__c ) )
        changesMap.put( cli.CommissionRecord__c, new List<String>() );
      changesMap.get( cli.CommissionRecord__c ).add( cli.Description__c + ' was added' );
    }
    makeChatterPostsAboutChanges( changesMap );
  }

  public static void createRecordsFromERPs( Set<Id> erpIds )
  {
    CommissionLineItem__c[] clis = buildNewRecords( erpIds );
    insert clis;
  }

  private static List<CommissionLineItem__c> buildNewRecords( Set<Id> materialIds )
  {
    /* result container List */
    CommissionLineItem__c[] lineItems = new List<CommissionLineItem__c>();

    /* Map<ErpOrderId, List<Materials>> */
    Map<Id, AcctSeedERP__Material__c[]> erp_Materials_Map = fetch_ERP_Materials_Map( materialIds);

    /* Map< erpOrderId, List<CommissionRecord>> */
    Map<Id, List<CommissionRecord__c>> comRecordMap = fetchCommissionRecords( erp_Materials_Map.keySet() );
    if( comRecordMap.size() > 0 )
    {
      for( Id erpId : comRecordMap.keySet() )
      {
        System.debug( '&&&&& erpId = ' + erpId );
        if( erp_Materials_Map.containsKey( erpId ) )
        {
          for( AcctSeedERP__Material__c mat : erp_Materials_Map.get( erpId ) )
          {
            CommissionLineItem__c cli = newFromProjectMaterial(mat);
            System.debug( '&&&&& cli = ' + cli);
            CommissionLineItem__c allInAdjustment = null;
            CommissionLineItem__c bonusLine = null;
            if( cli != null )
            {
              for( CommissionRecord__c cr : comRecordMap.get( erpId) )
              {
                if( cr != null )
                {
                  if( cr.Stage__c != 'New')
                  {
                    cli.hasChanged__c = true;
                  }
                  if( cr.Type__c == 'Retail Sales' && cli.Department__c == 'Sales' )
                  {
                    cli.CommissionRecord__c = cr.Id;
                    if( mat.AcctSeedERP__Product__r.RecordType.Name == 'Boat' )
                    {
                      allInAdjustment = allInPriceAdjustment( mat );
                      allInAdjustment.CommissionRecord__c = cr.Id;
                    }
                    if( mat.AcctSeedERP__Product__r.Commission_Bonus__c != null )
                    {
                      bonusLine = bonusLine( mat );
                      bonusLine.CommissionRecord__c = cr.Id;
                    }
                  }
                  if( cr.Type__c == 'Retail Counter Sale'  && cli.Department__c == 'Counter Sale' )
                    cli.CommissionRecord__c = cr.Id;

                  if( cr.Type__c == 'Business Office'  && cli.Department__c == 'Business Office' )
                    cli.CommissionRecord__c = cr.id;

                  if( cr.Type__c == 'Service Order' )
                    cli.CommissionRecord__c = cr.Id;
                }
              }
              if( cli.CommissionRecord__c != null )
                lineItems.add( cli );
              if( allInAdjustment != null )
                lineItems.add( allInAdjustment );
              if( bonusLine != null )
                lineItems.add( bonusLine );
            }
          }
        }
      }
    }
    return lineItems;
  }


  private static List<CommissionLineItem__c> updateRecords( Set<Id> materialIds )
  {
    Map<Id, CommissionLineItem__c> comLineItemMap = fetchCommissionLineItems( materialIds );
    List<CommissionLineItem__c> clis = new List<CommissionLineItem__c>();
    AcctSeedERP__Material__c[] materials = fetchMaterials( materialIds );
    for( AcctSeedERP__Material__c mat : materials )
    {
      if( comLineItemMap.containsKey(mat.Id) )
      {
        CommissionLineItem__c cli = newFromProjectMaterial( mat );
        if( cli != null )
        {
          cli.Id = comLineItemMap.get( mat.Id ).Id;
          cli.CommissionRecord__c = comLineItemMap.get( mat.Id ).CommissionRecord__c;
        }
        clis.add( cli );
      }
    }
    return clis;
  }

  public static void deleteRecords( Set<Id> materialIds )
  {
    Map<Id, List<String>> changesMap = new Map<Id, List<String>>();
    CommissionLineItem__c[] clis = [SELECT Id,
                                           CommissionRecord__c,
                                           Description__c
                                    FROM CommissionLineItem__c
                                    WHERE Material__c
                                    IN :materialIds];
    for( CommissionLineItem__c cli : clis )
    {
      if( !changesMap.containsKey( cli.CommissionRecord__c ) )
        changesMap.put( cli.CommissionRecord__c, new List<String>() );
      changesMap.get( cli.CommissionRecord__c ).add( cli.Description__c + ' was removed' );
    }
    delete clis;
    makeChatterPostsAboutChanges( changesMap );
  }

  public static void materialChanged(Map<Id, AcctSeedERP__Material__c> newMap,
                                     Map<Id, AcctSeedERP__Material__c> oldMap )
  {
    Set<Id> matId_toDelete = new Set<Id>();
    Set<Id> matId_toCreate = new Set<Id>();
    Set<Id> matId_toUpdate = new Set<Id>();
    Map<Id, List<String>> changesMap = new Map<Id, List<String>>();
    List<CommissionLineItem__c> clis = new List<CommissionLineItem__c>();
    for( AcctSeedERP__Material__c mat : newMap.values() )
    {
      AcctSeedERP__Material__c oldMat = oldMap.get( mat.Id );

      /* if price goes from 0 to anything other then 0 create new line item */
      if( mat.GMBLASERP__Unit_Price__c != 0 && oldMat.GMBLASERP__Unit_Price__c == 0 )
        matId_toCreate.add( mat.Id );
      /* if price goes to 0 from anything other than delete comlineItem */
      else if( mat.GMBLASERP__Unit_Price__c == 0 && oldMat.GMBLASERP__Unit_Price__c != 0 )
        matId_toDelete.add( mat.Id );
      /* everything else is an update */
      else if( mat.GMBLASERP__Unit_Price__c != oldMat.GMBLASERP__Unit_Price__c ||
               mat.AcctSeedERP__Quantity_Per_Unit__c != oldMat.AcctSeedERP__Quantity_Per_Unit__c )
        matId_toUpdate.add( mat.Id );
    }

    if( matId_toCreate.size() > 0 )
      clis.addAll( buildNewRecords( matId_toCreate ) );
    if( matId_toUpdate.size() > 0 )
      clis.addAll( updateRecords( matId_toUpdate ) );
    if( matId_toDelete.size() > 0 )
    {
      deleteRecords( matId_toDelete );
    }

    if( clis.size() > 0 )
    {
      for( CommissionLineItem__c cli : clis )
      {
        cli.hasChanged__c = true;
        System.debug('&&&&&' + cli.CommissionRecord__c );
        if( !changesMap.containsKey( cli.CommissionRecord__c) )
          changesMap.put( cli.CommissionRecord__c, new List<String>() );
        if( matId_toCreate.contains( cli.Material__c) )
          changesMap.get( cli.CommissionRecord__c ).add( cli.Description__c + ' has been added');
        if( matId_toUpdate.contains( cli.Material__c) )
        {
          AcctSeedERP__Material__c oldRec = oldMap.get( cli.Material__c );
          AcctSeedERP__Material__c newRec = newMap.get( cli.Material__c );
          if( newRec.GMBLASERP__Unit_Price__c != oldRec.GMBLASERP__Unit_Price__c )
          {
            changesMap.get( cli.CommissionRecord__c ).add(
              'Sale Price on ' + cli.Description__c + ' has changed from $' + oldRec.GMBLASERP__Unit_Price__c.setScale(2) + ' to $' + newRec.GMBLASERP__Unit_Price__c.setScale(2)
            );
          }
          if( newRec.AcctSeedERP__Quantity_Per_Unit__c != oldRec.AcctSeedERP__Quantity_Per_Unit__c )
          {
            changesMap.get( cli.CommissionRecord__c ).add(
              'Quantity of ' + cli.Description__c + ' has changed from  ' + oldRec.AcctSeedERP__Quantity_Per_Unit__c + ' to ' + newRec.AcctSeedERP__Quantity_Per_Unit__c
            );
          }
        }
      }
      upsert clis;
      makeChatterPostsAboutChanges( changesMap );
    }
  }

  private static void makeChatterPostsAboutChanges( Map<Id, List<String>> changeMap )
  {
    CommissionRecordERP.openCommissionsIfClosed( changeMap.keySet() );
    List<ConnectApi.BatchInput> batchInputs = new List<ConnectApi.BatchInput>();
    for( Id comRecId : changeMap.keySet() )
    {
      ConnectApi.BatchInput bi = new ConnectApi.BatchInput(
        buildChangesChatterPost( comRecId, changeMap.get( comRecId) )
      );
      batchInputs.add( bi );
    }
    if( batchInputs.size() > 0 && !Test.isRunningTest() )
    {
      ConnectApi.ChatterFeeds.postFeedElementBatch(Network.getNetworkId(), batchInputs);
    }
  }

  private static ConnectApi.FeedItemInput buildChangesChatterPost( Id recordId, List<String> changes )
  {
    ConnectApi.FeedItemInput feedItemInput = new ConnectApi.FeedItemInput();
    ConnectApi.MessageBodyInput messageBodyInput = new ConnectApi.MessageBodyInput();
    ConnectApi.MarkupBeginSegmentInput markupBeginSegment;
    ConnectApi.MarkupEndSegmentInput markupEndSegment;
    ConnectApi.TextSegmentInput textSegment = new ConnectApi.TextSegmentInput();
    messageBodyInput.messageSegments = new List<ConnectApi.MessageSegmentInput>();


    /* HEADER */
    markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
    markupBeginSegment.markupType = ConnectApi.MarkupType.Bold;
    messageBodyInput.messageSegments.add(markupBeginSegment);

    textSegment = new ConnectApi.TextSegmentInput();
    textSegment.text = 'The following changes where made:\n\n';
    messageBodyInput.messageSegments.add( textSegment );

    markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
    markupEndSegment.markupType = ConnectApi.MarkupType.Bold;
    messageBodyInput.messageSegments.add(markupEndSegment);

    /* BLANK LINE */
    markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
    markupBeginSegment.markupType = ConnectApi.MarkupType.Paragraph;
    messageBodyInput.messageSegments.add(markupBeginSegment);

    textSegment = new ConnectApi.TextSegmentInput();
    textSegment.text = '&nbsp;';
    messageBodyInput.messageSegments.add( textSegment );

    markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
    markupEndSegment.markupType = ConnectApi.MarkupType.Paragraph;
    messageBodyInput.messageSegments.add(markupEndSegment);

     /* ACTUAL NOTES */

     /* begin unOrderedList */
    markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
    markupBeginSegment.markupType = ConnectApi.MarkupType.UnorderedList;
    messageBodyInput.messageSegments.add(markupBeginSegment);

    /* create listItem for each change */
    for( String change: changes )
    {
      markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
      markupBeginSegment.markupType = ConnectApi.MarkupType.ListItem;
      messageBodyInput.messageSegments.add(markupBeginSegment);
      textSegment = new ConnectApi.TextSegmentInput();
      textSegment.text = change;
      messageBodyInput.messageSegments.add( textSegment );
      markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
      markupEndSegment.markupType = ConnectApi.MarkupType.ListItem;
      messageBodyInput.messageSegments.add(markupEndSegment);
    }

    markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
    markupEndSegment.markupType = ConnectApi.MarkupType.UnorderedList;
    messageBodyInput.messageSegments.add(markupEndSegment);

    feedItemInput.body = messageBodyInput;
    feedItemInput.feedElementType = ConnectApi.FeedElementType.FeedItem;
    feedItemInput.subjectId = recordId;
    return feedItemInput;
  }



