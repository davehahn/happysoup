  private static Decimal determineAdjustmentAmount( AcctSeedERP__Material__c item)
  {
    if( item.AcctSeedERP__Product__r.Family == 'Deck Boat' )
    {
      return -1700;
    }
    if( item.AcctSeedERP__Product__r.Family == 'Pontoon' )
    {
      return -1700;
    }
    if( item.AcctSeedERP__Product__r.Family == 'Side Console' )
    {
      return -1500;
    }
    if( item.AcctSeedERP__Product__r.Family == 'Full Windshield' )
    {
      return -1500;
    }
    if( item.AcctSeedERP__Product__r.Family == 'Utility' )
    {
      if( item.AcctSeedERP__Product__r.Name.contains('Boat Only') )
      {
        return -250;
      }
      return -1000;
    }
    return 0;
  }

  private static Decimal determineCommissionRate(AcctSeedERP__Material__c item)
  {
    Id recordTypeId = item.AcctSeedERP__Project__r.RecordTypeId;
    String saleType = item.AcctSeedERP__Project__r.Sale_Type__c;
    Id counterSaleRtId = gcConstants.idRtProjectCounterSale;
    Id serviceRtId = gcConstants.idRtProjectService;
    Id retailBoatSaleRtId = gcConstants.idRtProjectBoatSale;
    Set<Id> commissionableRTs = new Set<Id>{
      counterSaleRtId,
      serviceRtId,
      retailBoatSaleRtId
    };
    if( commissionableRTs.contains( recordTypeId ) )
    {
      if( recordTypeId == counterSaleRtId || recordTypeId == serviceRtId )
        return retailCounterSaleCommissionRate;

      if( recordTypeId == retailBoatSaleRtId )
        return retailCommissionRate( item );
    }
    else
    {
      //throw new CommissionRecordERP.CommissionRecordERPException( 'Commissions are not yet enabled for Partner Sales' );
      return null;
    }
    return null;
  }

  private static Decimal retailCommissionRate( AcctSeedERP__Material__c item )
  {
    Set<String> l1 = new Set<String>{'Boat'};
    Decimal l1Rate = 1.75;
    Set<String> l2 = new Set<String>{'motor', 'trailer', 'trollingmotor', 'part'};
    Decimal l2Rate = 10;
    //Set<String> packages = new Set<String>{'Luxury Package', 'Premium Package'};
    //Decimal packageRate = 1.75;
    Decimal freeLooseItemRate = -25;
    Decimal pAndSrate = 30;
    Decimal warranty = 30;
    Decimal discount = 10;
    if( item.GMBLASERP__Unit_Price__c < 0 )
    {
      return discount;
    }
    if( item.AcctSeedERP__Product__r.InsurancePlan__c != null )
    {
      return insuranceCommissionRate;
    }
    if( item.AcctSeedERP__Product__r.RecordType.Name == 'Protection and Services')
    {
      if( item.AcctSeedERP__Product__r.Family != null && item.AcctSeedERP__Product__r.Family.contains('Warranty') )
      {
        return warranty;
      }
      return pAndSrate;
    }
    if( l1.contains(item.AcctSeedERP__Product__r.RecordType.Name) )
    {
      return l1Rate;
    }
    if( l2.contains(item.AcctSeedERP__Product__r.RecordType.Name.toLowerCase().replace(' ', '') ) )
    {
      //if( packages.contains(item.AcctSeedERP__Product__r.Family) )
      //{
      //  return packageRate;
      //}
      if( item.GMBLASERP__Unit_Price__c == 0 )
      {
        return freeLooseItemRate;
      }
      return l2Rate;
    }
    return 0;
  }

  private static Map<String, Decimal> determinePrices(AcctSeedERP__Material__c item)
  {
    Map<String, Decimal> priceMap = new Map<String, Decimal>();
    if( item.AcctSeedERP__Product__r.Family != null && item.AcctSeedERP__Product__r.Family.contains('Warranty') )
    {
      Decimal d =  item.GMBLASERP__Unit_Price__c * 0.5;
      priceMap.put('retail', d );
      priceMap.put('sale', item.GMBLASERP__Unit_Price__c);
      priceMap.put('comAmount', d );
    }
    else if( item.AcctSeedERP__Product__r.InsurancePlan__c != null )
    {
      Decimal profitPercent =  item.AcctSeedERP__Product__r.Insurance_Profit_Percent_Override__c != null ?
        item.AcctSeedERP__Product__r.Insurance_Profit_Percent_Override__c :
        item.AcctSeedERP__Product__r.InsurancePlan__r.Profit_Percent__c;
      Decimal commissionableAmount = ( item.GMBLASERP__Unit_Price__c * ( profitPercent / 100 ) );

      priceMap.put('retail', item.GMBLASERP__Unit_Price__c == null ? 0.00 : item.GMBLASERP__Unit_Price__c);
      priceMap.put('sale', commissionableAmount);
      priceMap.put('comAmount', commissionableAmount);
    }
    else
    {
      priceMap.put('retail', item.GMBLASERP__Unit_Price__c == null ? 0.00 : item.GMBLASERP__Unit_Price__c);
      priceMap.put('sale', item.GMBLASERP__Unit_Price__c == null ? 0.00 : item.GMBLASERP__Unit_Price__c);
      priceMap.put('comAmount', item.GMBLASERP__Total_Price__c == null ? 0.00 : item.GMBLASERP__Total_Price__c);
    }

    return priceMap;
  }

  private static String buildDescription( AcctSeedERP__Material__c item )
  {
    Set<String> recordTypes = new Set<String>{'Boat', 'Motor', 'Trailer', 'Trolling Motor'};

    // *************** HERE ************************
    if( recordTypes.contains( item.AcctSeedERP__Product__r.RecordType.Name ) )
    {
      return item.AcctSeedERP__Comment__c + ' (' + item.AcctSeedERP__Product__r.Name + ')';
    }
    if( item.GMBLASERP__Unit_Price__c < 0 )
    {
      return item.AcctSeedERP__Product__r.Name + ' (' + item.AcctSeedERP__Comment__c + ')';
    }
    if( item.AcctSeedERP__Comment__c == null )
    {
      return item.AcctSeedERP__Product__r.Name;
    }
    return item.AcctSeedERP__Comment__c;
  }

  private static String determineDepartment( AcctSeedERP__Material__c item )
  {
    Id recordTypeId = item.AcctSeedERP__Project__r.RecordTypeId;
    Id counterSaleRtId = gcConstants.idRtProjectCounterSale;
    Id serviceRtId = gcConstants.idRtProjectService;
    Id retailBoatSale = gcConstants.idRtProjectBoatSale;
    Set<Id> businessOfficeProductRTs = new Set<Id>{
      LGND_Constants.productRtId_Protection,
      LGND_Constants.productRtId_Bundle,
      LGND_Constants.productRtId_Insurance
    };
    /* +++ Retail +++ */

    if( recordTypeId == counterSaleRtId )
      return 'Counter Sale';
    if( recordTypeId == serviceRtId )
      return 'Service Order';
    if( recordTypeId == retailBoatSale )
    {
      if( businessOfficeProductRTs.contains( item.AcctSeedERP__Product__r.RecordTypeId ) )
      {
        return 'Business Office';
      }
      else
      {
        return 'Sales';
      }
    }
    return null;
  }

