  public class ProvincialTax {
    public String id { get; set; }
    public String name { get; set; }
    public Decimal rate { get; set; }
    public Decimal retailRate { get; set; }
    public Boolean isHarmonized { get; set; }
    public String taxName { get; set; }
    public String taxNameFrench { get; set; }
    public Boolean isTaxable { get; set; }
    public AcctSeed__Accounting_Variable__c accountVariableRecord { get; set; }

    public ProvincialTax( AcctSeed__Accounting_Variable__c aVar)
    {
      setAttributes(aVar);
    }

    public ProvincialTax( Id aVarId )
    {
      setAttributes( [SELECT Id,
                            Name,
                            Tax_is_Harmonized__c,
                            Tax_Rate_Provincial__c,
                            Tax_Rate_Retail__c,
                            Uniquefier__c
                     FROM AcctSeed__Accounting_Variable__c
                     WHERE Id = :aVarId] );
    }

    private void setAttributes( AcctSeed__Accounting_Variable__c aVar )
    {
      this.id = aVar.Id;
      this.name = aVar.Name;
      this.rate = aVar.Tax_Rate_Provincial__c;
      this.retailRate = aVar.Tax_Rate_Retail__c;
      this.isHarmonized = aVar.Tax_is_Harmonized__c;
      this.isTaxable = aVar.Uniquefier__c == 'NonTaxable' ? false : true;
      this.accountVariableRecord = aVar;
      determineTaxName( aVar );
    }

    private void determineTaxName(AcctSeed__Accounting_Variable__c aVar)
    {
      if( aVar.Tax_is_Harmonized__c == true )
      {
        this.taxName = 'HST';
        this.taxNameFrench = 'TVH';
      }
      else if( aVar.Name == 'Quebec' )
      {
        this.taxName = 'QST';
        this.taxNameFrench = 'TVQ';
      }
      else
      {
        this.taxName = 'PST';
        this.taxNameFrench = 'PST';
      }
    }
  }

