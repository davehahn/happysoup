  public static Map<String, ProvincialTax> fetchProvincialTaxInfo()
  {
    Map<String, ProvincialTax> result = new Map<String, ProvincialTax>();
    for( AcctSeed__Accounting_Variable__c aVar : [SELECT Id,
                                                         Name,
