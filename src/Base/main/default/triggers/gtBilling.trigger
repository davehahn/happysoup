trigger gtBilling on AcctSeed__Billing__c (after delete, after insert, after undelete, after update, before delete, before insert, before update) {

	if(!gcBilling.disableTrigger)
	{
		gcBilling.triggerHandler
			(trigger.oldMap,
			 trigger.newMap,
			 trigger.new,
			 trigger.isBefore,
			 trigger.isInsert,
			 trigger.isUpdate,
			 trigger.isDelete);
	}
}