<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>Notify_Sales_Manager_of_Complete_Retail_Inspection</fullName>
        <description>Notify Sales Manager of Complete Retail Inspection</description>
        <protected>false</protected>
        <recipients>
            <recipient>mmcguire@legendboats.com</recipient>
            <type>user</type>
        </recipients>
        <senderType>DefaultWorkflowUser</senderType>
        <template>To_Do_Notification_Templates/A_new_Retail_Inspection_has_been_submitted</template>
    </alerts>
    <rules>
        <fullName>Notify Sales Manager of Completed Retail Inspection</fullName>
        <actions>
            <name>Notify_Sales_Manager_of_Complete_Retail_Inspection</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Legend_Inspection__c.CreatedDate</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <triggerType>onCreateOnly</triggerType>
    </rules>
</Workflow>
