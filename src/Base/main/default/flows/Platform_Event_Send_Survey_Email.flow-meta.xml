    <textTemplates>
        <name>SurveyEmail_English_CounterSale_Service</name>
        <isViewedAsPlainText>true</isViewedAsPlainText>
        <text>&lt;p&gt;&lt;img src=&quot;https://legend-media-vault.s3.us-west-2.amazonaws.com/images/NPS+Thank+You+Banner+EN.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(0, 0, 0); font-size: 14px;&quot;&gt;We are inviting you to participate in a quick survey because you are a valuable client to us here at Legend Boats.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(0, 0, 0); font-size: 14px;&quot;&gt;We actively use feedback to constantly improve ourselves and provide you with the best possible service because we know that your boat is your baby.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(0, 0, 0); font-size: 14px;&quot;&gt;This will take as little as 2 minutes and you will really be helping us out!&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(0, 0, 0); font-size: 14px;&quot;&gt;Please click on the link below to get started.&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;a href=&quot;https://legendboats.getfeedback.com/r/V9cP9zBg?AccountID={!Get_ERP_Order.AcctSeed__Account__c}&amp;amp;AccountName={!Get_ERP_Order.AcctSeed__Account__c}&amp;amp;ServiceRep={!Get_ERP_Order.Salesperson__c}&amp;amp;Warehouse={!Get_ERP_Order.GMBLASERP__Warehouse__c}&amp;amp;GLV1={!Get_ERP_Order.GL_Account_Variable_1__r.Name}&amp;amp;GLV2={!Get_ERP_Order.GL_Account_Variable_2__r.Name}&amp;amp;ERPCreateDate={!Get_ERP_Order.CreatedDate}&amp;amp;CreatedDate={!Get_ERP_Order.CreatedDate}&amp;amp;ERPId={!Get_ERP_Order.Id}&amp;amp;SalespersonId={!Get_ERP_Order.Salesperson__c}&amp;amp;gf_unique={!Get_ERP_Order.Id}&amp;amp;gf_expire={!Survey30DayExpiry}&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot;&gt;&lt;img src=&quot;https://legendboats--c.na152.content.force.com/servlet/servlet.ImageServer?id=0151Y00000F0ixu&amp;amp;oid=00Di0000000aRhm&quot;&gt;&lt;/a&gt;&lt;/p&gt;</text>
    </textTemplates>
    <textTemplates>
        <name>SurveyEmail_English_RetailBoatSale</name>
        <isViewedAsPlainText>true</isViewedAsPlainText>
        <text>&lt;p&gt;&lt;img src=&quot;https://legend-media-vault.s3.us-west-2.amazonaws.com/images/NPS+Thank+You+Banner+EN.jpg&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 14px; color: rgb(0, 0, 0);&quot;&gt;Hello {!Get_ERP_Order.AcctSeed__Account__r.FirstName},&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 14px; font-family: Arial, sans-serif; color: rgb(0, 0, 0);&quot;&gt;Thank&amp;nbsp;you for choosing Legend Boats!&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 14px; font-family: Arial, sans-serif; color: rgb(0, 0, 0);&quot;&gt;We are always working to make your ownership experience as amazing as possible, so we would love to hear from you.&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 14px; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255); font-family: Arial, sans-serif;&quot;&gt;This survey is only 7 questions short and shouldn&apos;t take more than a couple of minutes to complete. Your feedback is very important to us and we promise that every comment will be used to help create the best memories possible!&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 14px; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255); font-family: Arial, sans-serif;&quot;&gt;Thank you so much for your time. Happy Boating!&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;a href=&quot;https://legendboats.getfeedback.com/r/2bCtHsDZ?AccountID={!Get_ERP_Order.AcctSeed__Account__c}&amp;amp;AccontName={!Get_ERP_Order.AcctSeed__Account__c}&amp;amp;Email={!Get_ERP_Order.ContactEmail__c}&amp;amp;PickupLocation={!Get_ERP_Order.Pickup_Location__c}&amp;amp;SalesPerson={!Get_ERP_Order.Salesperson__c}&amp;amp;BoatSpecialist={!Get_ERP_Order.Boat_Specialist__c}&amp;amp;BOM={!Get_ERP_Order.Business_Manager__c}&amp;amp;PrimaryCampaignSource={!Get_ERP_Order.Primary_Campaign_Source_Name__c}&amp;amp;DeliveryDate={!Get_ERP_Order.Delivery_Date__c}&amp;amp;GLV1={!Get_ERP_Order.GL_Account_Variable_1__r.Name}&amp;amp;GLV2={!Get_ERP_Order.GL_Account_Variable_2__r.Name}&amp;amp;ERPCreateDate={!Get_ERP_Order.CreatedDate}&amp;amp;ERPId={!Get_ERP_Order.Id}&amp;amp;SalespersonId={!Get_ERP_Order.Salesperson__c}&amp;amp;BoatSpecialistId={!Get_ERP_Order.Boat_Specialist__c}&amp;amp;gf_unique={!Get_ERP_Order.Id}&amp;amp;gf_expire={!Survey30DayExpiry}&quot; rel=&quot;noopener noreferrer&quot; target=&quot;_blank&quot;&gt;&lt;img src=&quot;https://legendboats--c.na152.content.force.com/servlet/servlet.ImageServer?id=0151Y00000F0ixu&amp;amp;oid=00Di0000000aRhm&quot;&gt;&lt;/a&gt;&lt;/p&gt;</text>
    </textTemplates>
