<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Savings_Retail_Age__c</fullName>
    <description>Retail Age Savings Formula.</description>
    <externalId>false</externalId>
    <formula>- FLOOR(  Current_Retail_Price__c   * Retail_Age_Savings_Percent__c / IF(Current_Retail_Price__c&gt;25000, 500, 250)) * IF(Current_Retail_Price__c&gt;25000, 500, 250)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Retail Age Savings Formula. 
Current Retail Price x Retail Age Savings %</inlineHelpText>
    <label>Savings: Retail Age</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
