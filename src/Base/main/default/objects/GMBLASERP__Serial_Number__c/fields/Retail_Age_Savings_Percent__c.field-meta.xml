<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Retail_Age_Savings_Percent__c</fullName>
    <description>2.5% per 90 days, up to 15%</description>
    <externalId>false</externalId>
    <formula>IF(  ISPICKVAL(Status__c, &quot;Specials List&quot;) || ISPICKVAL(Status__c, &quot;Demo&quot;), MIN(ROUND(Retail_Age__c / 90,0)* 0.025, 0.15), 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>2.5% per 90 days, up to 15%</inlineHelpText>
    <label>Retail Age Savings %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
