        <script type="text/javascript">
            Handlebars.registerHelper("ifvalue", function(conditional, options) {
                if (conditional == options.hash.equals) {
                    return options.fn(this);
                } else {
                    return options.inverse(this);
                }
            });
            var resourceURL = '{!URLFOR( $Resource.slds283, "/icons/custom/")}';
            Handlebars.registerHelper('statusMethod', function(status){
                var htm;
                if(status == 'Display Model'){
                    htm =  '<span title="'+status+'" class="slds-icon_container slds-icon-custom-26 iconTitleTooltip"><img src="'+resourceURL+'custom26_60.png" /></span>';
                }else if(status == 'Demo'){
                    htm =  '<span title="'+status+'" class="slds-avatar__initials slds-icon-custom-38 iconTitleTooltip"><img src="'+resourceURL+'custom38_60.png" /></span>';
                }else if(status == 'Specials List'){
                    htm =  '<span title="'+status+'" class="slds-avatar__initials slds-icon-custom-93 iconTitleTooltip"><img src="'+resourceURL+'custom93_60.png" /></span>';
                }else if(status == 'To Be Repaired'){
                    htm =  '<span title="'+status+'" class="slds-avatar__initials slds-icon-custom-19 iconTitleTooltip"><img src="'+resourceURL+'custom19_60.png" /></span>';
                }else if(status == 'Used'){
                    htm =  '<span title="'+status+'" class="slds-avatar__initials slds-icon-custom-13 iconTitleTooltip"><img src="'+resourceURL+'custom13_60.png" /></span>';
                }else if(status == 'Boat Show'){
                    htm =  '<span title="'+status+'" class="slds-avatar__initials slds-icon-custom-61 iconTitleTooltip"><img src="'+resourceURL+'custom61_60.png" /></span>';
                }else{
                    htm =  '<span title="In Inventory" class="slds-avatar__initials slds-icon-custom-48 iconTitleTooltip"><img src="'+resourceURL+'custom48_60.png" /></span>';
                }
                return new Handlebars.SafeString(htm);
            });
        </script>

