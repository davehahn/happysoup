    <script>
        $(document).tooltip({
            items: "span.iconTitleTooltip"
        });
        // $( document ).on("mouseover",".iconTitleTooltip",function(){
        //     $(this).tooltip({
        //         content : function(){
        //             return $(this).attr('title');
        //         }
        //     });
        // });
        $(document).ready(function() {
            $('.lgnd').css({
                'max-height' : '100%',
                'overflow' : 'none'
            })
            $('.lgnd').show();
            initTab();

            ///
            $(document).on( "focus", "input.autoComplete2", function( event ) {
                $currentSelection = null;
                $inputField = $(this);
                $inputField.autocomplete({
                    minLength: 0,
                    autoFocus: false,
                    source: function (request, response) {
                        var idWholesaleErp = $inputField.data('whsid');
                        var idWholesalePrd = $inputField.data('prdid');
                        // console.log(idWholesaleErp);
                        Visualforce.remoting.Manager.invokeAction(
                            '{!$RemoteAction.InventoryAllocationController.retrieveRetailERPOrders}',
                             request.term, idWholesaleErp, idWholesalePrd,
                            function (result, event) {
                                if (event.status) {
                                    $.each(result,function(k,item){
                                        if(item.label == undefined)
                                            result[k].label = item.label;
                                        if(item.value == undefined)
                                            result[k].value = item.idPr;
                                    })  
                                    response(result);
                                } else {
                                    alert(event.message);
                                }
                            }, {
                                buffer: true,
                                escape: true,
                                timeout: 120000
                            });
                    },
                    focus: function (event, ui) {
                        $inputField.val(ui.item.label);
                        $currentSelection = ui.item;
                        return false;
                    },
                    select: function (event, ui) {
                        var idWholesaleErp = $inputField.data('whsid');
                        var idWholesalePrd = $inputField.data('prdid');
                        updateRetailERP(ui.item,idWholesaleErp,idWholesalePrd,false);
                        $currentSelection = null;
                        return false;
                    },
                    change: function (event, ui) {
                        var idWholesaleErp = $inputField.data('whsid');
                        var idWholesalePrd = $inputField.data('prdid');
                        if($currentSelection != null)
                        {
                            updateRetailERP($currentSelectionrowIndx,idWholesaleErp,idWholesalePrd, false);
                            $currentSelection = null;
                        }
                        return false;
                    }
                });
            });
        });

        function updateRetailERP(obj,idWholesaleERP,idWholesalePrd,allowTag)
        {
            var idPR = null;
            if(obj != null)
                idPR = obj.idPr;
            else{
                return;
            }
            showLoading();
            Visualforce.remoting.Manager.invokeAction(
                '{!$RemoteAction.InventoryAllocationController.updateRetailERP}',
                idWholesaleERP, idPR,idWholesalePrd, allowTag,
                function(result,event)
                {
                    if(event.status)
                    {
                        $('.retailSearch').dialog('destroy'); 
                        reloadThisRow(result.idProduct,result.lotName);
                        // $('div.dataTables_length_warehouse select').trigger('change');
                    }
                    else
                    {
                        if(event.message == 'NOMATCH'){
                            askForConfirmation(obj, idWholesaleERP,idWholesalePrd);
                            hideLoading();
                        }
                        else{
                            alert(event.message);
                            hideLoading();
                        }
                    }
                },
            {buffer:true, escape:true, timeout:120000});
        }

        function askForConfirmation(obj,idWholesaleERP,idWholesalePrd) {
            $('<div></div>').appendTo('body')
            .html('<div><h6>The wholesale ERP order lines for serialized items does not match with Retail ERP. Are you sure you want to proceed?</h6></div>')
            .dialog({
                modal: true, title: 'Confirmation', zIndex: 10000, autoOpen: true,
                width: 'auto', resizable: false,
                buttons: {
                    Yes: function () {
                        updateRetailERP(obj, idWholesaleERP,idWholesalePrd, true);
                        $(this).dialog("close");
                    },
                    No: function () {
                        $(this).dialog("close");
                    }
                },
                close: function (event, ui) {
                    $(this).remove();
                }
            });
        }

        function launchSelectRetail(ele)
        {
            $('.lgnd').find('.dynamic-details').remove();
            $ele = $(ele);
            var idWholesaleErp = $ele.data('iderp');
            var idProduct = $ele.data('idprod');
            console.log(idProduct);
            var $dialogDiv = $('<div class="retailSearch" style=\"z-index:100 !important;\"></div>');
            var $gridDiv = $('<div id=\"retailERPOrders\"></div>');
            var $retailList = $('<input class="slds-input autoComplete2" data-whsid=\"'+idWholesaleErp+'\" data-prdid=\"'+idProduct+'\" />');
            $gridDiv.append($retailList);
            // console.log(idWholesaleErp);
            // return;

            $dialogDiv.append($gridDiv);

            $dialogDiv.dialog({
                autoOpen: false,
                height: 100,
                modal: true,
                close:function(){
                    $gridDiv.remove(); 
                    $dialogDiv.dialog('destroy'); 
                    $dialogDiv.remove(); 
                },
                title: 'Select Retail'
            });

            $dialogDiv.dialog('open');
        }
        
        function clearRetail(ele)
        {
            $('.lgnd').find('.dynamic-details').remove();
            $ele = $(ele);
            var idWholesaleERP = $ele.data('iderp');
            var idWholesaleProd = $ele.data('idprod');
            console.log('idWholesaleProd');
            console.log(idWholesaleProd);
            showLoading();
            Visualforce.remoting.Manager.invokeAction(
                '{!$RemoteAction.InventoryAllocationController.clearRetailERP}',
                idWholesaleERP,idWholesaleProd,
                function(result,event)
                {
                    console.log(result);
                    if(event.status)
                    {
                        reloadThisRow(result.idProduct,result.lotName);
                        // $('div.dataTables_length_warehouse select').trigger('change');
                    }
                    else
                    {
                        alert(event.message);
                        hideLoading();
                    }
                },
            {buffer:true, escape:true, timeout:120000});
        }
        
        var retailOrderTemplate = Handlebars.compile( $('#retail-order-details-tmpl').html() ),
            ERPTemplate = Handlebars.compile( $('#erp-order-details-tmpl').html() ),
            retailERPTemplate = Handlebars.compile( $('#retail-erp-order-details-tmpl').html() ),
            wholesaleTemplate = Handlebars.compile( $('#wholesale-details-tmpl').html() ),
            SpecialsTemplate = Handlebars.compile( $('#specials-details-tmpl').html() ),
            MaterialTemplate = Handlebars.compile( $('#erp-materials-tmpl').html() ),
            RowTemplate = Handlebars.compile( $('#table-row-tmpl').html() );
        var iconMap = {
          partnerERP: "{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#account')}",
          retailERP: "{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#person_account')}",
          opportunity: "{!URLFOR($Asset.SLDS, 'assets/icons/standard-sprite/svg/symbols.svg#opportunity')}"
        };

        Handlebars.registerHelper( 'recordSize', function(records) {
          return new Handlebars.SafeString( records.length)
        });

        Handlebars.registerHelper( 'recordIcon', function( saleType) {
          return new Handlebars.SafeString( iconMap[saleType]);
        });

        $('.lgnd').on('change', '.reservedShipment', function(e) {
            var $this = $(this),
                shipId = $this.data('ship-id'),
                prodId = $this.data('product-id'),
                lot = $this.data('lot'),
                resv = $this.val();
            updateReservedShipment( shipId, resv, prodId, lot )
            .then(
                function() {
                    updateShipmentsTable(shipId, prodId,lot);
                },
                function(err) {
                    alert(err);
                }
            );
        });

        $('.lgnd').on('mouseenter', '.specials-boxes', function(e) {
          e.preventDefault();
          showpopupSpecials( $(this) );
        })
        .on( 'mouseleave', '.specials-boxes', function(e) {
          $(this).find('.dynamic-details').remove();
        });

        $('.lgnd').on('mouseenter', '.popup-orders', function(e) {
          e.preventDefault();
          showpopupOrders( $(this) );
        })
        .on( 'mouseleave', '.popup-orders', function(e) {
          $(this).find('.dynamic-details').remove();
        });

        $('.lgnd').on('mouseenter', '.popup-orders-2', function(e) {
          e.preventDefault();
          showpopupOrders2( $(this) );
        })
        .on( 'mouseleave', '.popup-orders-2', function(e) {
