public with sharing class BoatBuilderLineItem {
  public String product_id;
  public String description { get; set; }
  public String description_FR { get; set; }
  public Decimal quantity { get; set; }
  public Decimal quan { get; set; }
  public Decimal quantity_included { get; set; }
  public Decimal individual_price;
  public String product_family { get; set; }
  public String pricebook_entry_id { get; set; }
  public Decimal taxRate { get; set; }
  public List<String> features {
    get {
      if (features == null) {
        features = new List<String>();
      }
      return features;
    }
    set;
  }
  public List<String> featuresFR {
    get {
      if (featuresFR == null) {
        featuresFR = new List<String>();
      }
      return featuresFR;
    }
    set;
  }
  public String upgrade_cost {
    get {
      if (quantity == null || quantity_included == null) {
        return null;
      }
      if (individual_price == null) {
        return 'Price Not Set';
      }
      if (quantity == quantity_included) {
        if (product_family == 'Premium Package')
          return String.valueOf(individual_price);
        return 'Included';
      }
      return String.valueOf(individual_price * (quantity - quantity_included));
    } //getter method
  } //upgrade_cost
  public Map<String, BoatBuilderLineItem> subItems {
    get {
      if (subItems == null) {
        subItems = new Map<String, BoatBuilderLineItem>();
      }
      return subItems;
    }
    set;
  }
  //public List<BoatBuilderLineItem> subItemsList {
  //  get
  //  {
  //    if(subItemsList == null)
  //    {
  //      subItemsList = new List<BoatBuilderLineItem>();
  //    }
  //    return subItemsList;
  //  }
  //  set;
  //}
  public Decimal totalTaxes {
    get {
      return upgrade_cost == null || taxRate == null ? 0 : stringToDecimal(upgrade_cost) * taxRate;
    }
    set;
  }

  public BoatBuilderLineItem() {
  }

  public List<BoatBuilderLineItem> getSubItemValues() {
    return subItems.values();
  }

  private decimal stringToDecimal(String str) {
    try {
      return Decimal.valueOf(str);
    } catch (Exception e) {
      return 0;
    }
  }

