  private static List<Truck_Trip__c> fetchScheduables() {
    if (Schema.sObjectType.Truck_Trip__c.isUpdateable()) {
      return [SELECT Id, Name, Truck__c, Trailer__c, Driver__c FROM Truck_Trip__c WHERE Departure_Date__c = NULL];
    }
    return null; //new List<Truck_Trip__c>();
  }

  public static String findSchedulingConflicts(Truck_Trip__c tt) {
    Truck_Driver__c driver = [
      SELECT Id, AllowSchedulingConflicts__c
      FROM Truck_Driver__c
      WHERE Id = :tt.Truck_Driver__c
    ];
    if (!driver.AllowSchedulingConflicts__c) {
      List<String> errors = new List<String>();
      Truck_Trip__c[] conflicts = [
        SELECT Id, Truck_Driver__c, Truck_Driver__r.Name, Trailer__c, Truck__c, Departure_Date__c, Return_Date__c
        FROM Truck_Trip__c
        WHERE
          Id != :tt.Id
          AND ((Return_Date__c >= :tt.Departure_Date__c
          AND Departure_Date__c <= :tt.Departure_Date__c)
          OR (Departure_Date__c <= :tt.Return_Date__c
          AND Return_Date__c >= :tt.Return_Date__c))
          AND (Truck_Driver__c = :tt.Truck_Driver__c
          OR Truck__c = :tt.Truck__c
          OR Trailer__c = :tt.Trailer__c)
      ];
      for (Truck_Trip__c conflict : conflicts) {
        if (tt.Truck_Driver__c != null && tt.Truck_Driver__c == conflict.Truck_Driver__c)
          errors.add(conflict.Truck_Driver__r.Name + ' is already scheduled');
        if (tt.Truck__c != null && tt.Truck__c == conflict.Truck__c)
          errors.add('Truck ' + tt.Truck__c + ' is already in use');
        if (tt.Trailer__c != null && tt.Trailer__c == conflict.Trailer__c)
          errors.add('Trailer ' + tt.Trailer__c + ' is already in use');
      }
      if (errors.size() > 0) {
        return String.join(errors, ', ');
      }
      return null;
    }
    return null;
  }

