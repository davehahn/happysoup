  public static Map<String, Map<String, String>> fetchTruckDrivers() {
    Map<String, Map<String, String>> drivers = new Map<String, Map<String, String>>();
    for (Truck_Driver__c td : [
      SELECT Id, Name, CalBgColor__c, CalBorderColor__c, CalTextColor__c
      FROM Truck_Driver__c
      ORDER BY Name DESC
    ]) {
      Map<String, String> details = new Map<String, String>();
      details.put('Id', td.Id);
      details.put('Name', td.Name);
      details.put('BgColor', td.CalBgColor__c);
      details.put('BorderColor', td.CalBorderColor__c);
      details.put('TextColor', td.CalTextColor__c);
      drivers.put(td.Id, details);
    }
    return drivers;
  }

  @RemoteAction
  @Readonly
  public static List<EventData> fetchAllTrips(Long startAt, Long endAt, Id truckDriverId) {
    Date sDateTime = DateTime.newInstance(startAt).date();
    Date eDateTime = DateTime.newInstance(endAt).date();
    System.debug(sDateTime);
    System.debug(eDateTime);
    List<EventData> result = new List<EventData>();
    List<Truck_Trip__c> trips = [
      SELECT
        Id,
        Name,
        Driver__c,
        Distance_KM__c,
        Truck_Driver__c,
        Truck__c,
        Trailer__c,
        Departure_Date__c,
        Return_Date__c
      FROM Truck_Trip__c
      WHERE Departure_Date__c >= :sDateTime AND Departure_Date__c <= :eDateTime AND Truck_Driver__c = :truckDriverId
    ];
    for (Truck_Trip__c trip : trips) {
      result.add(new EventData(trip));
    }
    return result;
  }

