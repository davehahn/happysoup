  private void setCrudPermissions() {
    Schema.DescribeSObjectResult tripObj = Schema.sObjectType.Truck_Trip__c;
    Schema.DescribeSObjectResult destObj = Schema.sObjectType.Truck_Destination__c;
    Schema.DescribeSObjectResult matObj = Schema.sObjectType.AcctSeedERP__Material__c;
    crudPermissions = new Map<String, Boolean>();
    crudPermissions.put('canEditTrips', tripObj.isUpdateable());
    crudPermissions.put('canDeleteTrips', tripObj.isDeletable());
    crudPermissions.put('canCreateDestinations', destObj.isCreateable());
    crudPermissions.put('canEditDestinations', destObj.isUpdateable());
    crudPermissions.put('canDeleteDestinations', destObj.isDeletable());
    crudPermissions.put('canEditMaterials', matObj.isUpdateable());
  }

