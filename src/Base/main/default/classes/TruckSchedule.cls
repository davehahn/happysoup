  @RemoteAction
  public static EventData changedByDays(String tripId, Integer numDays) {
    if (Schema.sObjectType.Truck_Trip__c.isUpdateable()) {
      Truck_Trip__c trip = [
        SELECT Id, Distance_KM__c, Departure_Date__c, Return_Date__c, Truck__c, Trailer__c, Truck_Driver__c
        FROM Truck_Trip__c
        WHERE Id = :tripId
      ];
      trip.Departure_Date__c = trip.Departure_Date__c.addDays(numDays);
      trip.Return_Date__c = trip.Return_Date__c.addDays(numDays);
      String errorMsg = findSchedulingConflicts(trip);
      if (errorMsg != null)
        throw new TruckScheduleException(errorMsg);

      update trip;
      return new EventData(trip);
    }
    throw new TruckScheduleException('You do not have the required access on the Truck Trip object');
  }

  @RemoteAction
  public static EventData addedDays(String tripId, Integer numDays) {
    if (Schema.sObjectType.Truck_Trip__c.isUpdateable()) {
      Truck_Trip__c trip = [
        SELECT Id, Distance_KM__c, Departure_Date__c, Return_Date__c, Truck__c, Trailer__c, Truck_Driver__c
        FROM Truck_Trip__c
        WHERE Id = :tripId
      ];
      trip.Return_Date__c = trip.Return_Date__c.addDays(numDays);
      String errorMsg = findSchedulingConflicts(trip);
      if (errorMsg != null)
        throw new TruckScheduleException(errorMsg);

      update trip;
      return new EventData(trip);
    }
    throw new TruckScheduleException('You do not have the required access on the Truck Trip object');
  }

  @RemoteAction
  public static List<Truck_Trip__c> unschedule(String tripId) {
    if (Schema.sObjectType.Truck_Trip__c.isUpdateable()) {
      Truck_Trip__c trip = [SELECT Id FROM Truck_Trip__c WHERE Id = :tripId];
      trip.Driver__c = null;
      trip.Distance_KM__c = null;
      trip.Departure_Date__c = null;
      trip.Return_Date__c = null;
      trip.Truck__c = null;
      trip.Trailer__c = null;
      update trip;
      return fetchScheduables();
    }
    throw new TruckScheduleException('You do not have the required access on the Truck Trip object');
  }

