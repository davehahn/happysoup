  }
  global gcProject_TimeCardLedger(ApexPages.StandardController c) {
  }

  @RemoteAction
  @ReadOnly
  global static list<map<string, string>> retrieveTimeCardLedger(Id idProject) {
    list<map<string, string>> listJSON = new List<map<string, string>>();

    for (AcctSeed__Time_Card_Day__c tcdx : [
      SELECT
        Id,
        AcctSeed__Date__c,
        AcctSeed__Hours__c,
        AcctSeed__Time_Card_Line__r.AcctSeed__Project_Task__c,
        AcctSeed__Time_Card_Line__r.AcctSeed__Project_Task__r.Name,
        AcctSeed__Time_Card_Line__r.AcctSeed__Project_Task__r.AcctSeed__GL_Account_Variable_3__r.Name,
        AcctSeed__Time_Card_Line__r.AcctSeed__Project__r.Name,
        AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card__r.AcctSeed__Employee__r.Name,
        AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card_Variable_1__r.Name,
        AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card_Variable_2__r.Name,
        Burden__c,
        Hours_RT__c,
        Hours_OT__c,
        Hours_DT__c,
        Wages__c
      FROM AcctSeed__Time_Card_Day__c
      WHERE AcctSeed__Time_Card_Line__r.AcctSeed__Project__c = :idProject
      ORDER BY
        AcctSeed__Date__c DESC,
        AcctSeed__Time_Card_Line__r.AcctSeed__Project_Task__r.AcctSeed__GL_Account_Variable_3__r.Name,
        AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card_Variable_2__r.Name
    ]) {
      map<string, string> m = new Map<string, string>();

      m.put('employeeName', tcdx.AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card__r.AcctSeed__Employee__r.Name);

      m.put('timeCardDate', tcdx.AcctSeed__Date__c.format());

      m.put('projectTaskName', tcdx.AcctSeed__Time_Card_Line__r.AcctSeed__Project_Task__r.Name);

      m.put(
        'glv3Name',
        tcdx.AcctSeed__Time_Card_Line__r.AcctSeed__Project_Task__r.AcctSeed__GL_Account_Variable_3__r.Name
      );
      m.put('tcv1Name', tcdx.AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card_Variable_1__r.Name);
      m.put('tcv2Name', tcdx.AcctSeed__Time_Card_Line__r.AcctSeed__Time_Card_Variable_2__r.Name);
      m.put('workOrderName', tcdx.AcctSeed__Time_Card_Line__r.AcctSeed__Project__r.Name);

      m.put('hours', String.valueof(tcdx.AcctSeed__Hours__c.setScale(2)));
      m.put('hoursRT', String.valueof(tcdx.Hours_RT__c.setScale(2)));
      m.put('hoursDT', String.valueof(tcdx.Hours_DT__c.setScale(2)));
      m.put('hoursOT', String.valueof(tcdx.Hours_OT__c.setScale(2)));
      m.put('wages', String.valueof(tcdx.Wages__c.setScale(2)));
      m.put('burden', String.valueOf(tcdx.Burden__c.setScale(2)));

      listJSON.add(m);
    }

    return listJSON;
  }

  @RemoteAction
  @ReadOnly
  global static List<Map<String, String>> retrieveTimeCardPeriods(Id idFirstCertifiedPeriod) {
    //Get the Time Card Period
    AcctSeed__Time_Card_Period__c firstTcp = [
      SELECT AcctSeed__Start_Date__c, AcctSeed__End_Date__c
      FROM AcctSeed__Time_Card_Period__c
      WHERE Id = :idFirstCertifiedPeriod
    ][0];

    List<Map<String, String>> listJSON = new List<Map<String, String>>();

    for (AcctSeed__Time_Card_Period__c tcp : [
      SELECT Id, Name, AcctSeed__End_Date__c, AcctSeed__Start_Date__c
      FROM AcctSeed__Time_Card_Period__c
      WHERE AcctSeed__End_Date__c < TODAY
      ORDER BY AcctSeed__Start_Date__c
    ]) {
      // Ship anything before the first certified period
      if (tcp.AcctSeed__Start_Date__c < firstTcp.AcctSeed__Start_Date__c)
        continue;

      Map<String, String> m = new Map<String, String>();

      m.put('tcpId', tcp.Id);
      m.put('tcpName', tcp.Name);
      m.put('tcpStartDate', tcp.AcctSeed__Start_Date__c.format());
      m.put('tcpEndDate', tcp.AcctSeed__End_Date__c.format());
      listJSON.add(m);
    }

    return listJSON;
  }

