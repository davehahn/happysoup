public with sharing class Product2DetailsExt {
  public Boolean renderEdit { get; private set; }
  public Boolean hasSpecs { get; private set; }
  private final ApexPages.StandardController std;

  public Product2DetailsExt(ApexPages.StandardController controller) {
    std = controller;
    renderEdit = false;
    hasSpecs = false;
    Set<String> fieldNamesTest = new Set<String>();
    List<String> fieldNames = new List<String>();
    Map<String, Schema.FieldSet> FsMap = Schema.SObjectType.Product2.fieldSets.getMap();
    for (Schema.FieldSet fs : FsMap.values()) {
      if (fs.getName().contains('Specifications')) {
        for (Schema.FieldSetMember f : fs.getFields()) {
          if (!fieldNamesTest.contains(f.getFieldPath())) {
            fieldNamesTest.add(f.getFieldPath());
            fieldNames.add(f.getFieldPath());
          }
        }
      }
    }
    controller.addFields(fieldNames);
    Product2 p = (Product2) std.getRecord();
    hasSpecs = new Set<String>{ 'Pontoon', 'Full Windshield', 'Side Console', 'Utility', 'Deck Boat' }
        .contains(p.Family)
      ? true
      : false;
  }

  //public List<Schema.PicklistEntry> getBoatTypeValues() {
  //  return Product2.Boat_Type__c.getDescribe().getPickListValues();
  //}

  public PageReference editSpecs() {
    renderEdit = !renderEdit;
    return null;
  }

  public PageReference saveSpecs() {
    try {
      std.save();
      renderEdit = false;
      return null;
    } catch (Exception e) {
      String msg = e.getMessage();
      System.debug('+++++++++++++++ ' + msg + ' ============');
      return null;
    }
  }

