    public List<Integer> resultsCounts { get; set; } // The count of each response to a question's choices
    public List<SelectOption> rowOptions { get; set; }
    public boolean noData { get; set; }

    public question(SurveyQuestionResponse__c sqa) {
      name = sqa.Survey_Question__r.Name;
      id = sqa.Survey_Question__r.Id;
      System.debug('in Question found one with id ' + id);
      question = sqa.Survey_Question__r.Question__c;
      answer = sqa.Response__c;
      orderNumber = String.valueOf(sqa.Survey_Question__r.OrderNumber__c + 1);
      choices = sqa.Survey_Question__r.Choices__c;
      required = sqa.Survey_Question__r.Required__c;
      questionType = sqa.Survey_Question__r.Type__c;
      rowLabelLeft = sqa.Survey_Question__r.RowLabelLeft__c;
      rowLabelRight = sqa.Survey_Question__r.RowLabelRight__c;
      singleOptionsForChart = ' ';
      if (sqa.Survey_Question__r.Type__c == 'Single Select--Vertical') {
        renderSelectRadio = 'true';
        singleOptions = stringToSelectOptions(choices);
        renderSelectCheckboxes = 'false';
        renderFreeText = 'false';
        renderSelectRow = 'false';
      } else if (sqa.Survey_Question__r.Type__c == 'Multi-Select--Vertical') {
        renderSelectCheckboxes = 'true';
        multiOptions = stringToSelectOptions(choices);
        renderSelectRadio = 'false';
        renderFreeText = 'false';
        renderSelectRow = 'false';
      } else if (
        sqa.Survey_Question__r.Type__c == 'Single Select--Horizontal' ||
        sqa.Survey_Question__r.Type__c == 'NPS--Horizontal'
      ) {
        renderSelectCheckboxes = 'false';
        rowOptions = stringToSelectOptions(choices);
        renderSelectRadio = 'false';
        renderFreeText = 'false';
        renderSelectRow = 'true';
        renderRowLabels = sqa.Survey_Question__r.RowLabelRight__c != null ||
          sqa.Survey_Question__r.RowLabelLeft__c != null
          ? 'true'
          : 'false';
      } else if (sqa.Survey_Question__r.Type__c == 'Free Text') {
        renderFreeText = 'true';
        renderSelectRadio = 'false';
        renderSelectCheckboxes = 'false';
        renderSelectRow = 'false';
      }
      //responses= getResponses();
    }

    /** Splits up the string as given by the user and adds each option
     *  to a list to be displayed as option on the Visualforce page
     *  param: str String as submitted by the user
     *  returns the List of SelectOption for the visualforce page
     */
    private List<SelectOption> stringToSelectOptions(String str) {
      String singleOptionsForChart = '';
      if (str == '') {
        return new List<SelectOption>();
      }
      strList = str.split('\n');

      List<SelectOption> returnVal = new List<SelectOption>();
      for (String s : strList) {
        if (s != '') {
          returnVal.add(new SelectOption(s, s, true));
          if (s != 'null' && s != null) {
            String sBis = s.replace(' ', '%20');
            singleOptionsForChart += s.trim() + '|';
          }
        }
      }
      singleOptionsForChart = singleOptionsForChart.substring(0, singleOptionsForChart.length() - 1);
      return returnVal;
    }
  }

  private List<Question> buildQuestionsAndAnswers(List<SurveyQuestionResponse__c> sqas) {
    List<Question> allQA = new List<Question>();

    Double old_OrderNumber = 0;
    Double new_OrderNumber;
    Double difference = 0;
    /* Make sure that the order number follow each other (after deleting a question, orders might not do so) */
    for (SurveyQuestionResponse__c qa : sqas) {
      new_OrderNumber = qa.Survey_Question__r.OrderNumber__c;
      difference = new_OrderNumber - old_OrderNumber - 1;
      if (difference > 0) {
        Double dd = double.valueOf(difference);
        Integer newOrderInt = dd.intValue();
        qa.Survey_Question__r.OrderNumber__c -= Integer.valueOf(newOrderInt);
      }
      old_OrderNumber = qa.Survey_Question__r.OrderNumber__c;
      question theQ = new question(qa);
      allQA.add(theQ);
    }
    return allQA;
  }

