public with sharing class ViewCompletedSurvey {
  //private ApexPages.StandardController std;
  public String surveyTakerId { get; set; }
  public final SurveyTaker__c surveyTaker;
  public Boolean renderTakenSurvey { get; private set; }
  public List<Question> qAndAs { get; private set; }

  public ViewCompletedSurvey(ApexPages.StandardController stdCtrl) {
    renderTakenSurvey = false;
    //surveyTakerId = Apexpages.currentPage().getParameters().get('id');
    surveyTakerId = stdCtrl.getId();
    if (surveyTakerId != null) {
      surveyTaker = [
        SELECT
          Id,
          Survey__r.Name,
          Survey__r.Hide_Survey_Name__c,
          Contact__r.Id,
          Contact__r.Name,
          Opportunity__c,
          (
            SELECT
              Id,
              Response__c,
              Survey_Question__r.Id,
              Survey_Question__r.Survey__c,
              Survey_Question__r.Name,
              Survey_Question__r.Choices__c,
              Survey_Question__r.Question__c,
              Survey_Question__r.Type__c,
              Survey_Question__r.Required__c,
              Survey_Question__r.OrderNumber__c,
              Survey_Question__r.RowLabelLeft__c,
              Survey_Question__r.RowLabelRight__c
            FROM Survey_Question_Answers__r
            ORDER BY Survey_Question__r.OrderNumber__c
          )
        FROM SurveyTaker__c
        WHERE Id = :surveyTakerId
      ];
      if (surveyTaker != null) {
        renderTakenSurvey = true;
        qAndAs = buildQuestionsAndAnswers(surveyTaker.Survey_Question_Answers__r);
      }
    }
  }

  public SurveyTaker__c getSurveyTaker() {
    return surveyTaker;
  }

  public Boolean getRenderSurvey() {
    return qAndAs != null && qAndAs.size() > 0 ? true : false;
  }

  public class question {
    public String name { get; set; }
    public String id { get; set; }
    public String question { get; set; }
    public String answer { get; set; }
    public String orderNumber { get; set; }
    public String choices { get; set; }
    // public String   selectedOption         {get;set;}
    // public List<String> selectedOptions    {get;set;}
    public List<SelectOption> singleOptions { get; set; }
    public List<SelectOption> multiOptions { get; set; }
    public Boolean required { get; set; }
    public String questionType { get; set; }
    public String surveyName { get; set; }
    public String rowLabelLeft { get; set; }
    public String rowLabelRight { get; set; }
    public String renderRowLabels { get; set; }
    public String renderFreeText { get; set; }
    public String renderSelectRadio { get; set; }
    public String renderSelectCheckboxes { get; set; }
    public String renderSelectRow { get; set; }
    public List<String> responses { get; set; }
    public String singleOptionsForChart { get; set; }
    public String qResultsForChart { get; set; }
