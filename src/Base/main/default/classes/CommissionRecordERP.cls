  public static void erpChanged(Map<Id, AcctSeed__Project__c> newMap, Map<Id, AcctSeed__Project__c> oldMap) {
    Id counterSaleRtId = gcConstants.idRtProjectCounterSale;
    //Id serviceRtId = gcConstants.idRtProjectService;
    //Id retailBoatSaleRtId = gcConstants.idRtProjectBoatSale;
    Map<Id, AcctSeed__Project__c> erpOrders = fetchErpOrderMap(newMap.keySet());
    Map<Id, AcctSeed__Project__c> toCreate = new Map<Id, AcctSeed__Project__c>();
    List<CommissionRecord__c> toDelete = new List<CommissionRecord__c>();
    for (AcctSeed__Project__c erp : erpOrders.values()) {
      AcctSeed__Project__c oldErp = oldMap.get(erp.Id);
      /* +++++ Counter Sale ERP Orders +++++ */
      if (erp.RecordTypeId == counterSaleRtId) {
        System.debug('&&&&& dealing with a Legend Counter Sale ERP');
        /* Counter Sale Type Changed */
        if (erp.Counter_Sale_Type__c != oldErp.Counter_Sale_Type__c) {
          /*
            If new Counter Sale Type == Orientation and there is not currently a
            Commission Record we will need to create one
          */
          System.debug('&&&&& old Counter Sale Type = ' + oldErp.Counter_Sale_Type__c);
          System.debug('&&&&& new Counter Sale Type = ' + erp.Counter_Sale_Type__c);

          if (erp.Counter_Sale_Type__c == 'Orientation' && erp.Commission_Records__r.size() == 0) {
            toCreate.put(erp.Id, erp);
          }
          /*
            if Counter Sale Type was Orientation and is no longer and the Sales Person
            does not recieve commission we need to delete the commission record
          */
          if (
            oldErp.Counter_Sale_Type__c == 'Orientation' &&
            erp.Salesperson__c != null &&
            erp.Salesperson__r.Receives_Commission__c == false &&
            erp.Commission_Records__r.size() > 0
          ) {
            toDelete.addAll(erp.Commission_Records__r);
          }
        }
        /* ===== Counter Sale Type Changed ===== */
      }
      /* ===== Counter Sale ERP Orders===== */
    }

    if (toCreate.size() > 0) {
      buildAndInsertNewRecords(toCreate);
      CommissionLineItemERP.createRecordsFromERPs(toCreate.keySet());
    }
    if (toDelete.size() > 0)
      delete toDelete;
  }

