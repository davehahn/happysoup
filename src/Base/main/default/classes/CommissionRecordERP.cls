  private static Map<Id, AcctSeed__Project__c> fetchErpOrderMap(Set<Id> erpIds) {
    return new Map<Id, AcctSeed__Project__c>(
      [
        SELECT
          Id,
          Name,
          RecordTypeId,
          Stage__c,
          Sale_Type__c,
          Business_Manager__c,
          Counter_Sale_Type__c,
          Salesperson__c,
          Salesperson__r.Receives_Commission__c,
          Internal_Labour_Service__c,
          (SELECT Id FROM Commission_Records__r)
        FROM AcctSeed__Project__c
        WHERE Id IN :erpIds
      ]
    );
  }

  private static void buildAndInsertNewRecords(Map<Id, AcctSeed__Project__c> erpOrders) {
    CommissionRecord__c[] comRecords = new List<CommissionRecord__c>();
    for (AcctSeed__Project__c erp : erpOrders.values()) {
      CommissionRecord__c[] defaultRecords = buildDefaultRecords(erp);
      System.debug('&&&&&& defaultRecords = ' + defaultRecords);
      if (defaultRecords != null && defaultRecords.size() > 0)
        comRecords.addAll(defaultRecords);
    }
    //CommissionRecord.disableTriggerHandler = true;
    insert comRecords;
    insert buildPaymentRecords(comRecords, erpOrders);
  }

  private static CommissionPayment__c[] buildPaymentRecords(
    CommissionRecord__c[] comRecords,
    Map<Id, AcctSeed__Project__c> erpOrders
  ) {
    System.debug('&&&&& buildPaymentRecords called');
    System.debug('&&&&& comRecords = ' + comRecords);
    CommissionPayment__c[] payments = new List<CommissionPayment__c>();
    for (CommissionRecord__c cr : comRecords) {
      CommissionPayment__c cp = new CommissionPayment__c(CommissionRecord__c = cr.Id);
      if (cr.Type__c == 'Retail Sales' || cr.Type__c == 'Retail Counter Sale' || cr.Type__c == 'Service Order') {
        cp.Owner__c = erpOrders.get(cr.ERP_Order__c).Salesperson__c;
      }
      if (cr.Type__c == 'Business Office') {
        cp.Owner__c = erpOrders.get(cr.ERP_Order__c).Business_Manager__c;
      }
      if (cr.Type__c == 'Partner Sales') {
        //Handle Partner Sales Payments here probably will be Salesperson__c
      }
      if (cr.Type__c == 'Partner Counter Sale') {
        //Handle Partner Counter Sale Payments here probably will be Salesperson__c
      }
      payments.add(cp);
    }
    return payments;
  }

  public static CommissionRecord__c fetchRecord(String recordId) {
    return [
      SELECT
        Id,
        CommissionTotal__c,
        OpportunityId__c,
        Stage__c,
        Type__c,
        (
          SELECT
            Id,
            AppliedCommissionAmount__c,
            AppliedCommissionRate__c,
            CommissionableAmount__c,
            CalculatedCommissionAmount__c,
            CommissionRate__c,
            CustomItem__c,
            Department__c,
            Description__c,
            Quantity__c,
            Retail_Price__c,
            Sale_Price__c,
            isEditable__c,
            RecordStage__c,
            InsuranceItemId__c,
            OpportunityLineItemId__c
          FROM CommissionLineItems__r
          ORDER BY AppliedCommissionAmount__c DESC
        ),
        (
          SELECT
            Id,
            Owner__c,
            Owner__r.Name,
            Owner__r.Commission_Percentage__c,
            Owner__r.FullPhotoUrl,
            PaymentPercent__c,
            PaymentAmount__c
          FROM CommissionPayments__r
        )
      FROM CommissionRecord__c
      WHERE Id = :recordId
    ];
  }

  public static List<CommissionRecord__c> buildDefaultRecords(AcctSeed__Project__c erp) {
    Id counterSaleRtId = gcConstants.idRtProjectCounterSale;
    Id partnerCounterSaleRtId = gcConstants.idRtPartnerPartsOrder;
    Id serviceRtId = gcConstants.idRtProjectService;
    Id retailBoatSaleRtId = gcConstants.idRtProjectBoatSale;
    //Id partnerBoatSaleRtId = gcConstants.isRtProjectPartnerBoatSale;
    List<CommissionRecord__c> records = new List<CommissionRecord__c>();
    for (String comType : findTypes()) {
      /* +++ RETAIL COUNTER SALE / PARTS ORDER +++ */
      if (erp.RecordTypeId == counterSaleRtId && comType == 'Retail Counter Sale') {
        /* ++ RETAIL +++ */
        if (
          erp.Counter_Sale_Type__c == 'Orientation' ||
          (erp.Salesperson__c != null &&
          erp.Salesperson__r.Receives_Commission__c == true)
        ) {
          records.add(buildRecord(erp, comType));
        }
      }

      /* +++ PARTNER COUNTER SALE / PARTS ORDER +++ */
      if (erp.RecordTypeId == partnerCounterSaleRtId && comType == 'Partner Counter Sale') {
        // Deal with Partner Parts Order Commissions if there is such a thing
      }

      /* +++ Service Order +++ */
      if (erp.RecordTypeId == serviceRtId && !erp.Internal_Labour_Service__c && comType == 'Service Order') {
        if (comType == 'Service Order')
          records.add(buildRecord(erp, comType));
      }

      /* +++ BOAT/LOOSE ITEMS  SALES OR ORDERS +++*/
      if (erp.RecordTypeId == retailBoatSaleRtId) {
        /* +++ RETAIL +++ */
        if (erp.Sale_Type__c == 'Retail' && (comType == 'Retail Sales' || comType == 'Business Office')) {
          records.add(buildRecord(erp, comType));
        }

        /* +++ PARTNER +++ */
        if (erp.Sale_Type__c == 'Partner' && comType == 'Partner Sales') {
          // deal with Partner boat orders here when that happens
        }
      }
    }
    return records;
  }

  public static CommissionRecord__c buildRecord(AcctSeed__Project__c project, String type) {
    return new CommissionRecord__c(ERP_Order__c = project.Id, Name = concatName(project.Name), Type__c = type);
  }

  public static void triggerHandler(
    map<Id, CommissionRecord__c> oldMap,
    map<Id, CommissionRecord__c> newMap,
    list<CommissionRecord__c> listNew,
    Boolean boolIsBefore,
    Boolean boolIsInsert,
    Boolean boolIsUpdate,
    Boolean boolIsDelete
  ) {
    if (!disableTriggerHandler) {
      if (oldMap == null)
        oldMap = new Map<Id, CommissionRecord__c>();
      if (newMap == null)
        newMap = new Map<Id, CommissionRecord__c>();
      if (listNew == null)
        listNew = new List<CommissionRecord__c>();

      //Things to do BEFORE
      if (boolIsBefore) {
        if (boolIsInsert) {
        }
        if (boolIsUpdate) {
          validateAndTimeStampStageChange(listNew, oldMap);
        }
        if (boolIsDelete) {
        }
      } else // well.. if it is not before it has to be AFTER
      {
        if (boolIsInsert) {
          CommissionLineItem__c[] clis = new List<CommissionLineItem__c>();
          Map<Id, Id> erp_comm_map = new Map<Id, Id>();
          for (CommissionRecord__c cr : listNew) {
            clis.addAll(createDefaultLineItems(cr));
            if (cr.Type__c == 'Retail Sales')
              erp_comm_map.put(cr.Erp_Order__c, cr.Id);
          }
          if (!Test.isRunningTest())
            createCommissionNotesFromOppy(erp_comm_map);
          insert clis;
        }
        if (boolIsUpdate) {
          chatterAboutChanges(listNew, oldMap);
        }
        if (boolIsDelete) {
        }
      }
    }
  }

  @future
  public static void openCommissionsIfClosed(Set<Id> comRecIds) {
    List<CommissionRecord__c> wasComplete = new List<CommissionRecord__c>();
    Set<String> resetStages = new Set<String>{ 'Sales Accepted', 'Approved' };
    for (CommissionRecord__c cr : [
      SELECT Id, Stage__c
      FROM CommissionRecord__c
      WHERE Id IN :comRecIds AND Stage__c IN :resetStages
    ]) {
      cr.Stage__c = 'New';
      wasComplete.add(cr);
    }
    if (wasComplete.size() > 0) {
      Boolean orig = setStageAsSystem;
      setStageAsSystem = true;
      update wasComplete;
      setStageAsSystem = orig;
    }
  }

  private static void validateAndTimeStampStageChange(
    CommissionRecord__c[] listNew,
    Map<Id, CommissionRecord__c> oldMap
  ) {
    for (CommissionRecord__c cRec : listNew) {
      String oldStage = oldMap.get(cRec.Id).Stage__c;
      if (cRec.Was_Auto_Accepted__c && !oldMap.get(cRec.Id).Was_Auto_Accepted__c) {
        cRec.Stage__c = 'Sales Accepted';
      }
      if (cRec.Stage__c != oldStage) {
        if (allowedToSetStage(cRec, oldMap.get(cRec.Id))) {
          if (cRec.Stage__c == 'New') {
            cRec.ReviewedBy__c = null;
            cRec.ReviewedOn__c = null;
            cRec.ApprovedBy__c = null;
            cRec.ApprovedOn__c = null;
            cRec.AcceptedBy__c = null;
            cRec.AcceptedOn__c = null;
            cRec.Was_Auto_Accepted__c = false;
          }

          //Accounting Reviewed
          if (cRec.Stage__c == 'Reviewed') {
            if (!(oldStage == 'Approved' && cRec.Is_Disputed__c)) {
              cRec.ReviewedBy__c = UserInfo.getUserId();
              cRec.ReviewedOn__c = Datetime.now();
            }
            cRec.ApprovedBy__c = null;
            cRec.ApprovedOn__c = null;
            cRec.AcceptedBy__c = null;
            cRec.AcceptedOn__c = null;
            cRec.Was_Auto_Accepted__c = false;
          }
          //Accounting Approved
          if (cRec.Stage__c == 'Approved') {
            cRec.ApprovedBy__c = UserInfo.getUserId();
            cRec.ApprovedOn__c = Datetime.now();
            cRec.AcceptedBy__c = null;
            cRec.AcceptedOn__c = null;
            cRec.Was_Auto_Accepted__c = false;
          }
          //Sales Accepted
          if (cRec.Stage__c == 'Sales Accepted') {
            if (!cRec.Was_Auto_Accepted__c) {
              cRec.AcceptedBy__c = UserInfo.getUserId();
            }
            cRec.AcceptedOn__c = Datetime.now();
          }
        }
      }
    }
  }

  private static Boolean allowedToSetStage(CommissionRecord__c cRec, CommissionRecord__c oldRecord) {
    if (setStageAsSystem || cRec.Was_Auto_Accepted__c)
      return true;

    //Already Accepted - Complete
    String oldStage = oldRecord.Stage__c;
    if (oldStage == 'Sales Accepted' && !LGND_Utilities.CustomPermissions.canReOpenCommissionRecord) {
      cRec.addError('You do not have permission to re-open the Commission Record once it has been Accepted by Sales');
      return false;
    }

    if (cRec.Stage__c == 'Reviewed') {
      if (
        cRec.Is_Disputed__c &&
        !oldRecord.Is_Disputed__c &&
        oldStage == 'Approved' &&
        Schema.sObjectType.CommissionRecord__c.fields.AcceptedBy__c.isUpdateable()
      ) {
        return true;
      }
      if (!Schema.sObjectType.CommissionRecord__c.fields.ReviewedBy__c.isUpdateable()) {
        cRec.addError('You do not have permission to update the record to ' + cRec.Stage__c);
        return false;
      }
    }
    if (cRec.Stage__c == 'Approved' && !Schema.sObjectType.CommissionRecord__c.fields.ApprovedBy__c.isUpdateable()) {
      cRec.addError('You do not have permission to update the record to ' + cRec.Stage__c);
      return false;
    }
    if (
      cRec.Stage__c == 'Sales Accepted' && !Schema.sObjectType.CommissionRecord__c.fields.AcceptedBy__c.isUpdateable()
    ) {
      cRec.addError('You do not have permission to update the record to ' + cRec.Stage__c);
      return false;
    }
    return true;
  }

  private static void createCommissionNotesFromOppy(Map<Id, Id> erp_comm_map) {
    List<ConnectApi.BatchInput> batchInputs = new List<ConnectApi.BatchInput>();
    for (AcctSeed__Project__c erp : [
      SELECT Id, AcctSeed__Opportunity__c, AcctSeed__Opportunity__r.Commission_Notes__c
      FROM AcctSeed__Project__c
      WHERE Id IN :erp_comm_map.keySet()
    ]) {
      if (erp.AcctSeed__Opportunity__c != null && erp.AcctSeed__Opportunity__r.Commission_Notes__c != null) {
        ConnectApi.BatchInput bi = new ConnectApi.BatchInput(
          makeChatterPost(erp.AcctSeed__Opportunity__r.Commission_Notes__c, erp_comm_map.get(erp.Id))
        );
        batchInputs.add(bi);
      }
    }
    if (batchInputs.size() > 0) {
      ConnectApi.ChatterFeeds.postFeedElementBatch(Network.getNetworkId(), batchInputs);
    }
  }

  public static ConnectApi.FeedItemInput makeChatterPost(String message, Id recordId) {
    ConnectApi.FeedItemInput feedItemInput = new ConnectApi.FeedItemInput();
    ConnectApi.MessageBodyInput messageBodyInput = new ConnectApi.MessageBodyInput();
    ConnectApi.MarkupBeginSegmentInput markupBeginSegment;
    ConnectApi.MarkupEndSegmentInput markupEndSegment;
    ConnectApi.TextSegmentInput textSegment = new ConnectApi.TextSegmentInput();

    messageBodyInput.messageSegments = new List<ConnectApi.MessageSegmentInput>();

    /* HEADER */
    markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
    markupBeginSegment.markupType = ConnectApi.MarkupType.Bold;
    messageBodyInput.messageSegments.add(markupBeginSegment);

    textSegment = new ConnectApi.TextSegmentInput();
    textSegment.text = 'Commission Notes:\n\n';
    messageBodyInput.messageSegments.add(textSegment);

    markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
    markupEndSegment.markupType = ConnectApi.MarkupType.Bold;
    messageBodyInput.messageSegments.add(markupEndSegment);

    /* BLANK LINE */
    markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
    markupBeginSegment.markupType = ConnectApi.MarkupType.Paragraph;
    messageBodyInput.messageSegments.add(markupBeginSegment);

    textSegment = new ConnectApi.TextSegmentInput();
    textSegment.text = '&nbsp;';
    messageBodyInput.messageSegments.add(textSegment);

    markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
    markupEndSegment.markupType = ConnectApi.MarkupType.Paragraph;
    messageBodyInput.messageSegments.add(markupEndSegment);

    /* ACTUAL NOTES */
    markupBeginSegment = new ConnectApi.MarkupBeginSegmentInput();
    markupBeginSegment.markupType = ConnectApi.MarkupType.Paragraph;
    messageBodyInput.messageSegments.add(markupBeginSegment);

    textSegment = new ConnectApi.TextSegmentInput();
    textSegment.text = message;
    messageBodyInput.messageSegments.add(textSegment);

    markupEndSegment = new ConnectApi.MarkupEndSegmentInput();
    markupEndSegment.markupType = ConnectApi.MarkupType.Paragraph;
    messageBodyInput.messageSegments.add(markupEndSegment);

    feedItemInput.body = messageBodyInput;
    feedItemInput.feedElementType = ConnectApi.FeedElementType.FeedItem;
    feedItemInput.subjectId = recordId;
    return feedItemInput;
  }

  public static Set<String> findTypes() {
    Set<String> plValues = new Set<String>();
    for (Schema.PicklistEntry ple : CommissionRecord__c.Type__c.getDescribe().getPicklistValues()) {
      plValues.add(ple.getValue());
    }
    return plValues;
  }

  private static List<CommissionLineItem__c> createDefaultLineItems(CommissionRecord__c cr) {
    CommissionLineItem__c[] clis = new List<CommissionLineItem__c>();
    if (cr.Type__c == 'Business Office') {
      clis.add(CommissionLineItemERP.bankReserveFee(cr.Id));
      clis.add(CommissionLineItemERP.adminFee(cr.Id));
    }
    return clis;
  }

  private static String concatName(String oppName) {
    if (oppName.length() < 80)
      return oppName;
    return oppName.subString(0, 79);
  }

  private static void chatterAboutChanges(CommissionRecord__c[] listNew, Map<Id, CommissionRecord__c> oldMap) {
    List<ConnectApi.BatchInput> batchInputs = new List<ConnectApi.BatchInput>();
    Map<Id, CommissionRecord__c> stageChangedMap = new Map<Id, CommissionRecord__c>();
    CommissionRecord__c[] totalChangedList = new List<CommissionRecord__c>();
    for (CommissionRecord__c cr : listNew) {
      CommissionRecord__c oldCr = oldMap.get(cr.Id);
      //Check for Stage Change
      if (cr.Stage__c != oldCr.Stage__c) {
        stageChangedMap.put(cr.Id, cr);
      }
      //Check for Total Change
      if (cr.CommissionTotal__c != oldCr.CommissionTotal__c) {
        totalChangedList.add(cr);
      }
    }
    if (stageChangedMap.size() > 0) {
      batchInputs.addAll(chatterAboutStageChange(stageChangedMap, oldMap));
    }
    if (totalChangedList.size() > 0) {
      batchInputs.addAll(chatterAboutTotalChange(totalChangedList, oldMap));
    }
    if (batchInputs.size() > 0 && !Test.isRunningTest()) {
      ConnectApi.ChatterFeeds.postFeedElementBatch(Network.getNetworkId(), batchInputs);
    }
  }

  private static List<ConnectApi.BatchInput> chatterAboutTotalChange(
    CommissionRecord__c[] changedList,
    Map<Id, CommissionRecord__c> oldMap
  ) {
    List<ConnectApi.BatchInput> batchInputs = new List<ConnectApi.BatchInput>();
    for (CommissionRecord__c cr : changedList) {
      CommissionRecord__c oldCr = oldMap.get(cr.Id);
      String message =
        '... changed the Commission Total from $' +
        oldCr.CommissionTotal__c +
        ' to $' +
        cr.CommissionTotal__c;
      ConnectApi.BatchInput batchInput = new ConnectApi.BatchInput(
        LGND_Utilities.makeChatterPost(null, message, cr.Id)
      );
      batchInputs.add(batchInput);
    }
    return batchInputs;
  }

  private static List<ConnectApi.BatchInput> chatterAboutStageChange(
    Map<Id, CommissionRecord__c> changedMap,
    Map<Id, CommissionRecord__c> oldMap
  ) {
    List<ConnectApi.BatchInput> batchInputs = new List<ConnectApi.BatchInput>();
    String message;
    List<Id> mentionUserIds;
    for (CommissionRecord__c cr : [
      SELECT Id, Stage__c, CommissionName__c, (SELECT Id, Owner__c, Owner__r.ManagerId FROM CommissionPayments__r)
      FROM CommissionRecord__c
      WHERE Id IN :changedMap.keyset()
    ]) {
      mentionUserIds = new List<Id>();
      CommissionRecord__c oldCr = oldMap.get(cr.Id);
      if ((oldCr.Stage__c != 'Sales Accepted' || oldCr.Stage__c != 'Approved') && cr.Stage__c == 'Approved') {
        message = ' Good News! This Commission Record has been Approved by Accounting. Please review and accept this Commission. The commission will be automatically accept on your behalf after 7 days.';
        for (CommissionPayment__c cp : cr.CommissionPayments__r) {
          mentionUserIds.add(cp.Owner__c);
        }
      }
      if ((oldCr.Stage__c != 'Sales Accepted' || oldCr.Stage__c != 'Approved') && cr.Stage__c == 'New') {
        message = ' The associated ERP has changed therefore changing this commission record.  All Approvals have been reset and the process must be completed again';
        if (oldCr.ApprovedBy__c != null) {
          mentionUserIds.add(oldCr.ApprovedBy__c);
        }
        if (oldCr.AcceptedBy__c != null) {
          mentionUserIds.add(oldCr.AcceptedBy__c);
        }
      }
      if (message != null && mentionUserIds.size() > 0) {
        ConnectApi.BatchInput batchInput = new ConnectApi.BatchInput(
          LGND_Utilities.makeChatterPost(mentionUserIds, message, cr.Id)
        );
        batchInputs.add(batchInput);
      }
    }
    return batchInputs;
  }

  private static List<Id> accountingUserIds() {
    List<Id> result = new List<Id>();
    for (
      PermissionSetAssignment psa : [
        SELECT Id, Name, (SELECT Id, AssigneeId FROM Assignments)
        FROM PermissionSet
        WHERE Name = 'Commission_Accounting_Approve'
      ]
      .Assignments
    ) {
      result.add(psa.AssigneeId);
    }
    return result;
  }

