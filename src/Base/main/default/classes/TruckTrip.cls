  private static void setDestinationsDevliveryDate(Map<Id, Truck_Trip__c> newMap, Map<Id, Truck_Trip__c> oldMap) {
    Truck_Destination__c[] changed = new List<Truck_Destination__c>();
    Truck_Destination__c[] destinations = [
      SELECT Id, Travel_Days__c, Delivery_Date__c, Trip__c
      FROM Truck_Destination__c
      WHERE Trip__c IN :newMAp.keySet()
    ];
    for (Truck_Destination__c td : destinations) {
      if (newMap.get(td.Trip__c).Departure_Date__c != oldMap.get(td.Trip__c).Departure_Date__c) {
        if (newMap.get(td.Trip__c).Departure_Date__c == null) {
          td.Delivery_Date__c = null;
        } else {
          Integer extraDays = td.Travel_Days__c <= 1
            ? 0
            : Integer.valueOf((td.Travel_Days__c / 2).round(System.RoundingMode.CEILING) - 1);
          td.Delivery_Date__c = newMap.get(td.Trip__c).Departure_Date__c.addDays(extraDays);
        }
        changed.add(td);
      }
    }
    TruckDestination.disableTriggerHandler = true;
    update changed;
  }

