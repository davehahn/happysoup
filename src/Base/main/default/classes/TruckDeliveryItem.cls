public with sharing class TruckDeliveryItem {
  public static Boolean disableTrigger {
    get {
      if (disableTrigger == null)
        return false;
      else
        return disableTrigger;
    }
    set {
      if (value == null)
        disableTrigger = false;
      else
        disableTrigger = value;
    }
  }

  public static void triggerHandler(
    Map<Id, Truck_Delivery_Item__c> oldMap,
    Map<Id, Truck_Delivery_Item__c> newMap,
    List<Truck_Delivery_Item__c> listNew,
    Boolean boolIsBefore,
    Boolean boolIsInsert,
    Boolean boolIsUpdate,
    Boolean boolIsDelete
  ) {
    if (!disableTrigger) {
      if (oldMap == null)
        oldMap = new Map<Id, Truck_Delivery_Item__c>();
      if (newMap == null)
        newMap = new Map<Id, Truck_Delivery_Item__c>();
      if (listNew == null)
        listNew = new List<Truck_Delivery_Item__c>();

      //Things to do BEFORE
      if (boolIsBefore) {
        if (boolIsInsert) {
        }
        if (boolIsUpdate) {
        }
        if (boolIsDelete) {
          Set<Id> erpIds = new Set<Id>();
          Set<Id> transferIds = new Set<Id>();
          Id partnerRtId = gcConstants.idRtProjectPartnerBoatSale;
          for (Truck_Delivery_Item__c tdi : oldMap.values()) {
            if (tdi.ERP_Order__c != null)
              erpIds.add(tdi.ERP_Order__c);
            if (tdi.Legend_Transfer_Request__c != null)
              transferIds.add(tdi.Legend_Transfer_Request__c);
          }
          AcctSeed__Project__c[] oldErps = [SELECT Id FROM AcctSeed__Project__c WHERE Id IN :erpIds];
          for (AcctSeed__Project__c erp : oldErps) {
            //if( erp.RecordTypeId == partnerRtId )
            //    erp.Stage__c = 'Plan Load';
            erp.Destination__c = null;
          }
          Legend_Transfer_Request__c[] transfers = [SELECT Id FROM Legend_Transfer_Request__c WHERE Id IN :transferIds];
          for (Legend_Transfer_Request__c t : transfers) {
            t.Stage__c = 'Plan Load';
            t.Destination__c = null;
          }
          update oldErps;
          update transfers;
        }
      } else // well.. if it is not before it has to be AFTER
      {
        if (boolIsInsert) {
          Map<Id, Id> erpDestMap = new Map<Id, Id>();
          Map<Id, Id> transferDestMap = new Map<Id, Id>();
          for (Truck_Delivery_Item__c tdi : listNew) {
            if (tdi.ERP_Order__c != null)
              erpDestMap.put(tdi.ERP_Order__c, tdi.Destination__c);
            if (tdi.Legend_Transfer_Request__c != null)
              transferDestMap.put(tdi.Legend_Transfer_Request__c, tdi.Destination__c);
          }
          Id partnerRtId = gcConstants.idRtProjectPartnerBoatSale;
          AcctSeed__Project__c[] erps = [
            SELECT Id, RecordTypeId
            FROM AcctSeed__Project__c
            WHERE Id IN :erpDestMap.keySet()
          ];
          for (AcctSeed__Project__c erp : erps) {
            //if( erp.RecordTypeId == partnerRtId )
            //    erp.Stage__c = 'Tagging Pending';
            erp.Destination__c = erpDestMap.get(erp.Id);
          }
          Legend_Transfer_Request__c[] transfers = [
            SELECT Id
            FROM Legend_Transfer_Request__c
            WHERE Id IN :transferDestMap.keySet()
          ];
          for (Legend_Transfer_Request__c t : transfers) {
            t.Stage__c = 'Scheduled';
            t.Destination__c = transferDestMap.get(t.Id);
          }
          update erps;
          update transfers;
        }
        if (boolIsUpdate) {
        }
        if (boolIsDelete) {
        }
      }
    }
  }

  public static void erpWasDeleted(Set<Id> erpIds) {
    Truck_Delivery_Item__c[] di = [SELECT Id FROM Truck_Delivery_Item__c WHERE ERP_Order__c IN :erpIds];
    delete di;
  }

