public with sharing class ERPDocument_Trigger {
  public static void doHandle(
    Map<Id, ERP_document__c> oldMap,
    Map<Id, ERP_document__c> newMap,
    List<ERP_document__c> listNew,
    Boolean boolIsBefore,
    Boolean boolIsInsert,
    Boolean boolIsUpdate,
    Boolean boolIsDelete
  ) {
    if (oldMap == null)
      oldMap = new Map<Id, ERP_document__c>();
    if (newMap == null)
      newMap = new Map<Id, ERP_document__c>();
    if (listNew == null)
      listNew = new List<ERP_document__c>();

    updateAndValidate(oldMap, newMap, listNew, boolIsBefore, boolIsInsert, boolIsUpdate, boolIsDelete);
    //Things to do BEFORE
    if (boolIsBefore) {
      if (boolIsInsert) {
      }
      if (boolIsUpdate) {
      }
      if (boolIsDelete) {
      }
    } else // well.. if it is not before it has to be AFTER
    {
      if (boolIsInsert) {
        insertLink(listNew);
      }
      if (boolIsUpdate) {
      }
      if (boolIsDelete) {
        Set<Id> docIds = new Set<Id>();
        for (ERP_document__c doc : oldMap.values()) {
          docIds.add(doc.DocumentId__c);
        }
        if (!docIds.isEmpty()) {
          List<Document> docs = [SELECT Id FROM Document WHERE Id IN :docIds];
          List<AcctSeed__Project__c> erps = [SELECT Id FROM AcctSeed__Project__c WHERE LastDocumentID__c IN :docIds];
          for (AcctSeed__Project__c erp : erps) {
            erp.LastDocumentID__c = null;
          }
          update erps;
          delete docs;
        }
      }
    }
  }

  private static void updateAndValidate(
    map<Id, ERP_document__c> oldMap,
    map<Id, ERP_document__c> newMap,
    list<ERP_document__c> listNew,
    Boolean boolIsBefore,
    Boolean boolIsInsert,
    Boolean boolIsUpdate,
    Boolean boolIsDelete
  ) {
    if (boolIsBefore && (boolIsUpdate || boolIsInsert)) {
      Map<String, Decimal> m = new Map<String, Decimal>();
      if (boolIsInsert)
        m = generateCertificateNumber();
      for (ERP_document__c doc : listNew) {
        if (doc.Report_Type__c != 'Invoice' && doc.Report_Type__c != null) {
          if (boolIsInsert) {
            Decimal lastCert = m.get(doc.Report_Type__c);
            if (lastCert == null)
              lastCert = 1;
            else
              lastCert = lastCert + 1;
            m.put(doc.Report_Type__c, lastCert);
            doc.Certificate_Number__c = lastCert;
          }
          if (
            boolIsUpdate &&
            doc.Certificate_Number__c != oldMap.get(doc.Id).Certificate_Number__c &&
            oldMap.get(doc.Id).Certificate_Number__c != null
          )
            doc.Certificate_Number__c = oldMap.get(doc.Id).Certificate_Number__c;
        }
      }
    }
  }

  public static Map<String, Decimal> generateCertificateNumber() {
    Map<String, Decimal> m = new Map<String, Decimal>();
    for (sObject doc : [
      SELECT Report_Type__c reportType, Max(Certificate_Number__c) maxCertNo
      FROM ERP_document__c
      WHERE (Report_Type__c != NULL OR Report_Type__c != 'Invoice') AND Certificate_Number__c != NULL
      GROUP BY Report_Type__c
    ])
      m.put((String) doc.get('reportType'), (Decimal) doc.get('maxCertNo'));
    return m;
  }

  public static void insertLink(List<ERP_document__c> erpDoc) {
    Map<String, String> docIds = new Map<String, String>();
    List<String> allProj = new List<String>();
    for (ERP_document__c doc : erpDoc) {
      if (doc.isInvoice__c == true) {
        docIds.put(doc.Project__c, doc.DocumentID__c);
        allProj.add(doc.Project__c);
      }
    }

    List<AcctSeed__Project__c> projects = [SELECT Id FROM AcctSeed__Project__c WHERE id IN :allProj];

    for (AcctSeed__Project__c p : projects) {
      p.LastDocumentID__c = docIds.get(p.id);
    }
    Boolean orig = gcProject.disableTrigger;
    gcProject.disableTrigger = true;
    update projects;
    gcProject.disableTrigger = orig;
  }

